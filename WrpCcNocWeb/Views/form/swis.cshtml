                                                                                            {<span>iii. Name of the Water Bodies to be Used</span>}
                                                                                        }
                                                                                    </label>
                                                                                    <div id="NameOfWaterBody" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModWaterBody> _wbd = ViewBag.LookUpCcModWaterBody;

                                                                                            if (_wbd.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModWaterBody wdb in _wbd)
                                                                                                {
                                                                                                    var id = "radNameOfWaterBody" + wdb.WaterBodyId;

                                                                                                    if (language == "1")
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyId" id="@id" value="@wdb.WaterBodyId" data-parsley-multiple="WaterBodyId">
                                                                                                            <label for="@id"> @wdb.NameOfWaterBodyBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyId" id="@id" value="@wdb.WaterBodyId" data-parsley-multiple="WaterBodyId">
                                                                                                            <label for="@id"> @wdb.NameOfWaterBody </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="RiverNature">
                                                                                        @{if (language == "1")
                                                                                            {<span>iv. নদীর ধরণ</span>}
                                                                                            else
                                                                                            {<span>iv. River Type</span>}
                                                                                        }
                                                                                    </label>
                                                                                    <div id="RiverNature" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModRiverType> _rvin = ViewBag.LookUpCcModRiverType;

                                                                                            if (_rvin.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModRiverType rn in _rvin)
                                                                                                {
                                                                                                    var id = "chkRiverType" + rn.RiverTypeId;

                                                                                                    if (language == "1")
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@rn.RiverTypeNameBn">
                                                                                                            <input type="checkbox" name="RiverTypeId" id="@id" value="@rn.RiverTypeId" data-parsley-multiple="RiverTypeId">
                                                                                                            <label for="@id"> @rn.RiverTypeNameBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
