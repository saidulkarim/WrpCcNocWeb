@using static WrpCcNocWeb.ViewModels.GeneralInfo
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var form_lan = HttpContextAccessor.HttpContext.Request.Cookies["FormLanguage"].ToString();
    form_lan = string.IsNullOrEmpty(form_lan) ? "2" : form_lan; //2 = default english
    var language = int.Parse(form_lan);
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3">
                    <b>@ViewBag.ProjectTitle</b>

                    <button id="btnGoBack" type="button" class="btn btn-secondary float-right" style="margin-top: -10px;">Back</button>

                    <button id="btnExpandAll" type="button" class="btn btn-danger waves-effect waves-light float-right mr-1"
                            style="margin-top: -10px;">
                        <span class="btn-label"><i class="mdi mdi-arrow-expand-vertical"></i></span>
                        @if (language == 1)
                        {<span>সব কিছু বিশদভাবে ব্যক্ত করুন</span>}
                        else
                        {<span>Expand All</span>}
                    </button>

                    <button id="btnCollapseAll" type="button" class="btn btn-info waves-effect waves-light float-right mr-1"
                            style="margin-top: -10px;">
                        <span class="btn-label"><i class="mdi mdi-arrow-collapse-vertical"></i></span>
                        @if (language == 1)
                        {<span>সব সঙ্কুচিত করুন</span>}
                        else
                        { <span>Collapse All</span>}
                    </button>
                </h4>

                <div id="basicwizard">
                    <ul class="nav nav-pills bg-light nav-justified form-wizard-header">
                        <li class="nav-item">
                            <a id="lnkTab1" href="#basictab1" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2 active">
                                <i class="mdi mdi-format-float-right mr-1"></i>
                                <span class="d-none d-sm-inline">
                                    @if (language == 1)
                                    {<span>আবেদন পত্র</span> }
                                    else
                                    {<span>Application Form</span>}
                                </span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a id="lnkTab2" href="#basictab2" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2">
                                <i class="mdi mdi-face-profile mr-1"></i>
                                <span class="d-none d-sm-inline">
                                    @if (language == 1)
                                    {<span>প্রিভিউ</span> }
                                    else
                                    {<span>Preview</span>}
                                </span>
                            </a>
                        </li>
                    </ul>

                    <div class="tab-content b-0 mb-0">
                        <div class="tab-pane active" id="basictab1">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="card-box">
                                        <div class="row">
                                            @Html.Hidden("ProjectId")
                                            @Html.Hidden("AppSubmissionId")
                                            @Html.Hidden("ApplicationStateId")
                                            @Html.Hidden("ApprovalStatusId")
                                            @Html.Hidden("Project37IndvId")
                                            @Html.Hidden("UserId")

                                            <input id="ProjectTypeId" type="hidden" value="@ViewBag.ProjectTypeId" />

                                            <div class="col-sm-3">
                                                @{ await Html.RenderPartialAsync("form/_1TabPanel"); }
                                            </div>

                                            <div class="col-sm-9">
                                                <div class="tab-content pt-0">
                                                    <div class="tab-pane fade active show" id="v-gen-info" role="tabpanel" aria-labelledby="v-gen-info-tab">
                                                        <form id="frmGeneralInfo">
                                                            <div class="card-box">
                                                                @{ await Html.RenderPartialAsync("form/_2GeneralInfo"); }
                                                            </div>
                                                        </form>
                                                    </div>

                                                    <div class="tab-pane fade" id="v-tech-info" role="tabpanel" aria-labelledby="v-tech-info-tab">
                                                        <form id="frmTechnicalInfo">
                                                            <div class="card-box">
                                                                <div id="tech-info-form">
                                                                    <div class="card">
                                                                        <div class="card-header bg-blue py-3 text-white cursor" data-toggle="collapse" href="#cardCollpase371">
                                                                            <div class="card-widgets">
                                                                                <a data-toggle="collapse" href="#cardCollpase371" role="button" aria-expanded="false" aria-controls="cardCollpase371"><i class="mdi mdi-minus"></i></a>
                                                                            </div>
                                                                            <h5 class="card-title mb-0 text-white">
                                                                                @if (language == 1)
                                                                                {<span>১. বেজলাইন সম্পর্কিত তথ্য বা অবস্থা পর্যালোচনা</span>}
                                                                                else
                                                                                { <span>1. Discussion About Baseline Related Information or Situation</span>}
                                                                            </h5>
                                                                        </div>
                                                                        <div id="cardCollpase371" class="collapse show">
                                                                            <div class="card-body">
                                                                                <div class="form-group">
                                                                                    <label for="IsSpecializedIndustrialZone">
                                                                                        @if (language == 1)
                                                                                        {<span>i. শিল্প প্রতিষ্ঠানটি কোন EPZ বা সরকার ঘোষিত বিশেষায়িত শিল্পাঞ্চলের অন্তভূক্ত কিনা?</span>}
                                                                                        else
                                                                                        {<span>i. Whether the Proposed Industry Falls Under Any EPZ or Government Declared Specialized Industrial Zone?</span>}
                                                                                    </label>
                                                                                    <div id="IsSpecializedIndustrialZone" class="row" style="margin-left: 5px;">
                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                            <input type="radio" name="IsSpecializedIndustrialZone" id="radIsSpecIndZoneYes" value="1" data-parsley-multiple="IsSpecializedIndustrialZone">
                                                                                            <label for="radIsSpecIndZoneYes">
                                                                                                @if (language == 1)
                                                                                                {<span>হ্যাঁ</span> }
                                                                                                else
                                                                                                { <span>Yes</span>}
                                                                                            </label>
                                                                                        </div>

                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                            <input type="radio" name="IsSpecializedIndustrialZone" id="radIsSpecIndZoneNo" value="0" data-parsley-multiple="IsSpecializedIndustrialZone">
                                                                                            <label for="radIsSpecIndZoneNo">
                                                                                                @if (language == 1)
                                                                                                {<span>না</span> }
                                                                                                else
                                                                                                { <span>No</span>}
                                                                                            </label>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div id="divDSWPD" class="row" style="margin-left: -7px;">
                                                                                    <label>
                                                                                        @if (language == 1)
                                                                                        {<span>ii. বেসলাইন সম্পর্কে আলোচনা হয়েছে?</span>}
                                                                                        else
                                                                                        {<span>ii. Discussed the Baseline?</span>}
                                                                                    </label>
                                                                                    <div class="table-responsive">
                                                                                        <table class="table mb-0">
                                                                                            <thead>
                                                                                                <tr>
                                                                                                    <th class="text-center">
                                                                                                        @if (language == 1)
                                                                                                        {<span>হ্যাঁ/ না</span> }
                                                                                                        else
                                                                                                        { <span>Yes/ No</span>}
                                                                                                    </th>
                                                                                                    <th class="text-center">
                                                                                                        @if (language == 1)
                                                                                                        {<span>আবেদনকারী মন্তব্য</span> }
                                                                                                        else
                                                                                                        { <span>Applicant Comments</span>}
                                                                                                    </th>
                                                                                                </tr>
                                                                                                <tr>
                                                                                                    <th class="text-center">
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="DiscussAboutBaselineYesNoId" id="chkDABYes" value="1" data-parsley-multiple="DiscussAboutBaselineYesNoId">
                                                                                                            <label for="chkDABYes"> Yes </label>
                                                                                                        </div>

                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="DiscussAboutBaselineYesNoId" id="chkDABNo" value="0" data-parsley-multiple="DiscussAboutBaselineYesNoId">
                                                                                                            <label for="chkDABNo"> No </label>
                                                                                                        </div>
                                                                                                    </th>
                                                                                                    <th class="text-center">
                                                                                                        <input type="text" id="DabApplicantComments" name="DabApplicantComments" class="form-control" />
                                                                                                    </th>
                                                                                                </tr>
                                                                                            </thead>
                                                                                        </table>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="HydrologicalRegion">
                                                                                        @if (language == 1)
                                                                                        {<span>iii. হাইড্রোলজিক্যাল অঞ্চল অনুযায়ী প্রকল্প এলাকা</span>}
                                                                                        else
                                                                                        {<span>iii. Project area according to Hydrological Region</span>}
                                                                                    </label>
                                                                                    <div id="chkHydrologicalRegions" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModHydroRegion> _hydro_regions = ViewBag.LookUpCcModHydroRegion;

                                                                                            if (_hydro_regions.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModHydroRegion hr in _hydro_regions)
                                                                                                {
                                                                                                    var id = "chkHydroRegion" + hr.HydroRegionId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullNameBn">
                                                                                                            <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                            <label for="@id"> @hr.HydroRegionFullNameBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullName">
                                                                                                            <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                            <label for="@id"> @hr.HydroRegionFullName </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="BDP2100HotSpot">
                                                                                        @if (language == 1)
                                                                                        {<span>iv. বাংলাদেশ ব-দ্বীপ পরিকল্পনা-২১০০ এর হট স্পট অনুযায়ী প্রকল্প এলাকা</span>}
                                                                                        else
                                                                                        {<span>iv. Project Area According to Bangladesh Delta Plan 2100 Hot Spot</span>}
                                                                                    </label>
                                                                                    <div id="chkBDP2100HotSpot" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModDeltPlan2100HotSpot> _hot_spots = ViewBag.LookUpCcModDeltPlan2100HotSpot;

                                                                                            if (_hot_spots.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModDeltPlan2100HotSpot hs in _hot_spots)
                                                                                                {
                                                                                                    var id = "chkBDP2100HotSpot" + hs.DeltaPlanHotSpotId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanNameBn">
                                                                                                            <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                            <label for="@id"> @hs.PlanNameBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanName">
                                                                                                            <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                            <label for="@id"> @hs.PlanName </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="NameOfWaterBody">
                                                                                        @if (language == 1)
                                                                                        {<span>v. পানি উত্তোলনের উৎস</span>}
                                                                                        else
                                                                                        {<span>v. Name of the Water Bodies to be Used</span>}
                                                                                    </label>
                                                                                    <div id="NameOfWaterBody" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModWaterBody> _wbd = ViewBag.LookUpCcModWaterBody;

                                                                                            if (_wbd.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModWaterBody wdb in _wbd)
                                                                                                {
                                                                                                    var id = "radNameOfWaterBody" + wdb.WaterBodyId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyId" id="@id" value="@wdb.WaterBodyId" data-parsley-multiple="WaterBodyId">
                                                                                                            <label for="@id"> @wdb.NameOfWaterBodyBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyId" id="@id" value="@wdb.WaterBodyId" data-parsley-multiple="WaterBodyId">
                                                                                                            <label for="@id"> @wdb.NameOfWaterBody </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="RiverNature">
                                                                                        @if (language == 1)
                                                                                        {<span>vi. নদীর প্রকৃতি</span>}
                                                                                        else
                                                                                        {<span>vi. River Nature</span>}
                                                                                    </label>
                                                                                    <div id="RiverNature" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModRiverType> _rvin = ViewBag.LookUpCcModRiverType;

                                                                                            if (_rvin.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModRiverType rn in _rvin)
                                                                                                {
                                                                                                    var id = "chkRiverType" + rn.RiverTypeId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@rn.RiverTypeNameBn">
                                                                                                            <input type="checkbox" name="RiverTypeId" id="@id" value="@rn.RiverTypeId" data-parsley-multiple="RiverTypeId">
                                                                                                            <label for="@id"> @rn.RiverTypeNameBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@rn.RiverTypeName">
                                                                                                            <input type="checkbox" name="RiverTypeId" id="@id" value="@rn.RiverTypeId" data-parsley-multiple="RiverTypeId">
                                                                                                            <label for="@id"> @rn.RiverTypeName </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="WaterBody">
                                                                                        @if (language == 1)
                                                                                        {<span>vii. প্রবাহের ধরন</span>}
                                                                                        else
                                                                                        {<span>vii. Type of Water Bodies</span>}
                                                                                    </label>
                                                                                    <div id="WaterBody" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModTypeOfWaterBody> _towb = ViewBag.LookUpCcModTypeOfWaterBody;

                                                                                            if (_towb.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModTypeOfWaterBody wb in _towb)
                                                                                                {
                                                                                                    var id = "radWaterBody" + wb.WaterBodyTypeId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyTypeId" id="@id" value="@wb.WaterBodyTypeId" data-parsley-multiple="WaterBodyTypeId">
                                                                                                            <label for="@id"> @wb.WaterBodyTypeBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyTypeId" id="@id" value="@wb.WaterBodyTypeId" data-parsley-multiple="WaterBodyTypeId">
                                                                                                            <label for="@id"> @wb.WaterBodyType </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div id="divOfftakeOutfallConnectivity">
                                                                                    <label for="OfftakeOutfallConnectivity" class="bold">
                                                                                        @if (language == 1)
                                                                                        {<span>viii. উৎস মুখ, পতিত মুখ এবং সংযোগ</span>}
                                                                                        else
                                                                                        {<span>viii. Offtake, Outfall and Connectivity</span>}
                                                                                    </label>

                                                                                    <hr style="margin-top: 0 !important;" />

                                                                                    <div class="row">
                                                                                        <div class="form-group col-xl-4 text-center">
                                                                                            <label for="Offtake">
                                                                                                @{if (language == 1)
                                                                                                    { <span>উৎস মুখ</span>}
                                                                                                    else
                                                                                                    {<span>Offtake</span>}
                                                                                                }
                                                                                            </label>
                                                                                            <input type="number" min="0" id="Offtake" name="Offtake" class="form-control" />
                                                                                        </div>

                                                                                        <div class="form-group col-md-4 text-center">
                                                                                            <label for="Outfall">
                                                                                                @{if (language == 1)
                                                                                                    { <span>পতিত মুখ</span>}
                                                                                                    else
                                                                                                    {<span>Outfall</span>}
                                                                                                }
                                                                                            </label>
                                                                                            <input type="number" min="0" id="Outfall" name="Outfall" class="form-control" />
                                                                                        </div>

                                                                                        <div class="form-group col-xl-4 text-center">
                                                                                            <label for="Connectivity">
                                                                                                @{if (language == 1)
                                                                                                    { <span>অন্যান্য নদ-নদী/ খাল বিলের সাথে সংযোগ</span>}
                                                                                                    else
                                                                                                    {<span>Connectivity with Other Waterbodies</span>}
                                                                                                }
                                                                                            </label>
                                                                                            <input type="number" min="0" id="Connectivity" name="Connectivity" class="form-control" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="HydrologicalSystem">
                                                                                        @if (language == 1)
                                                                                        {<span>ix. ইনটেক পয়েন্ট হতে নিকটস্থ নদী/খাল/ বিলের দূরত্ব</span>}
                                                                                        else
                                                                                        {<span>ix. Surrounding Water Bodies with Distance from Intake Point</span>}
                                                                                    </label>

                                                                                    <div class="row">
                                                                                        <div class="col-md-12">
                                                                                            @{ await Html.RenderPartialAsync("controls/_HydrologicalSystem"); }
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div id="divAnnualRainfall">
                                                                                    <label for="AnnualRainfall" class="bold">
                                                                                        @if (language == 1)
                                                                                        {<span>x. বার্ষিক বৃষ্টিপাত (বিগত পাঁচ বছর) (মি.মি.)</span>}
                                                                                        else
                                                                                        {<span>x. Annual Rainfall (last five years) (mm)</span>}
                                                                                    </label>

                                                                                    <hr style="margin-top: 0 !important;" />

                                                                                    <div class="row">
                                                                                        <div class="col-md-12">
                                                                                            @{ await Html.RenderPartialAsync("controls/_AnnualRainfall"); }
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="WaterLevelIntakePoint">
                                                                                        @if (language == 1)
                                                                                        {<span>xi. ইনটেক পয়েন্টে পানির সমতল (ওয়াটার লেভেল)</span>}
                                                                                        else
                                                                                        {<span>xi. Water Level (m PWD) at Intake Point</span>}
                                                                                    </label>

                                                                                    <div id="WaterLevelIntakePoint" class="row" style="margin-left: -7px;">
                                                                                        <div class="table-responsive">
                                                                                            <table class="table mb-0">
                                                                                                <thead>
                                                                                                    <tr>
                                                                                                        <th class="text-center" colspan="2">
                                                                                                            @if (language == 1)
                                                                                                            {<span>শুকনো মৌসুম</span>}
                                                                                                            else
                                                                                                            {<span>Dry Season</span>}
                                                                                                        </th>
                                                                                                        <th class="text-center" colspan="2">
                                                                                                            @if (language == 1)
                                                                                                            {<span>বর্ষা মৌসুম</span>}
                                                                                                            else
                                                                                                            {<span>Wet Season</span>}
                                                                                                        </th>
                                                                                                    </tr>
                                                                                                    <tr>
                                                                                                        <th class="text-center">
                                                                                                            @if (language == 1)
                                                                                                            {<span>সর্বোচ্চ</span>}
                                                                                                            else
                                                                                                            {<span>Maximum</span>}
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            @if (language == 1)
                                                                                                            {<span>সর্বনিম্ন</span>}
                                                                                                            else
                                                                                                            {<span>Minimum</span>}
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            @if (language == 1)
