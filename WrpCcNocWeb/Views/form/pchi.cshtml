@using static WrpCcNocWeb.ViewModels.GeneralInfo
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var form_lan = HttpContextAccessor.HttpContext.Request.Cookies["FormLanguage"].ToString();
    form_lan = string.IsNullOrEmpty(form_lan) ? "2" : form_lan; //2 = default english
    var language = int.Parse(form_lan);
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3">
                    <b>@ViewBag.ProjectTitle</b>

                    <button id="btnGoBack" type="button" class="btn btn-secondary float-right" style="margin-top: -10px;">Back</button>

                    <button id="btnExpandAll" type="button" class="btn btn-danger waves-effect waves-light float-right mr-1"
                            style="margin-top: -10px;">
                        <span class="btn-label"><i class="mdi mdi-arrow-expand-vertical"></i></span>
                        @if (language == 1)
                        {<span>সব কিছু বিশদভাবে ব্যক্ত করুন</span>}
                        else
                        {<span>Expand All</span>}
                    </button>

                    <button id="btnCollapseAll" type="button" class="btn btn-info waves-effect waves-light float-right mr-1"
                            style="margin-top: -10px;">
                        <span class="btn-label"><i class="mdi mdi-arrow-collapse-vertical"></i></span>
                        @if (language == 1)
                        {<span>সব সঙ্কুচিত করুন</span>}
                        else
                        { <span>Collapse All</span>}
                    </button>
                </h4>

                <div id="basicwizard">
                    <ul class="nav nav-pills bg-light nav-justified form-wizard-header">
                        <li class="nav-item">
                            <a id="lnkTab1" href="#basictab1" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2 active">
                                <i class="mdi mdi-format-float-right mr-1"></i>
                                <span class="d-none d-sm-inline">
                                    @if (language == 1)
                                    {<span>আবেদন পত্র</span> }
                                    else
                                    {<span>Application Form</span>}
                                </span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a id="lnkTab2" href="#basictab2" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2">
                                <i class="mdi mdi-face-profile mr-1"></i>
                                <span class="d-none d-sm-inline">
                                    @if (language == 1)
                                    {<span>প্রিভিউ</span> }
                                    else
                                    {<span>Preview</span>}
                                </span>
                            </a>
                        </li>
                    </ul>

                    <div class="tab-content b-0 mb-0">
                        <div class="tab-pane active" id="basictab1">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="card-box">
                                        <div class="row">
                                            @Html.Hidden("ProjectId")
                                            @Html.Hidden("AppSubmissionId")
                                            @Html.Hidden("ApplicationStateId")
                                            @Html.Hidden("ApprovalStatusId")
                                            @Html.Hidden("Project34IndvId")
                                            @Html.Hidden("UserId")

                                            <input id="ProjectTypeId" type="hidden" value="@ViewBag.ProjectTypeId" />

                                            <div class="col-sm-3">
                                                @{ await Html.RenderPartialAsync("form/_1TabPanel"); }
                                            </div> <!-- end col-->
                                            <div class="col-sm-9">
                                                <div class="tab-content pt-0">
                                                    <div class="tab-pane fade active show" id="v-gen-info" role="tabpanel" aria-labelledby="v-gen-info-tab">
                                                        <form id="frmGeneralInfo">
                                                            <div class="card-box">
                                                                @{ await Html.RenderPartialAsync("form/_2GeneralInfo"); }
                                                            </div>
                                                        </form>
                                                    </div>

                                                    <div class="tab-pane fade" id="v-tech-info" role="tabpanel" aria-labelledby="v-tech-info-tab">
                                                        <form id="frmTechnicalInfo">
                                                            <div class="card-box">
                                                                <div id="tech-info-form">
                                                                    <div class="card">
                                                                        <div class="card-header bg-blue py-3 text-white cursor" data-toggle="collapse" href="#cardCollpase31">
                                                                            <div class="card-widgets">
                                                                                <a data-toggle="collapse" href="#cardCollpase31" role="button" aria-expanded="false" aria-controls="cardCollpase2"><i class="mdi mdi-minus"></i></a>
                                                                            </div>
                                                                            <h5 class="card-title mb-0 text-white">
                                                                                @if (language == 1)
                                                                                {<span>১. বেজলাইন সম্পর্কিত তথ্য বা অবস্থা পর্যালোচনা</span>}
                                                                                else
                                                                                {<span>1. Discussion about baseline related information or situation</span>}
                                                                            </h5>
                                                                        </div>
                                                                        <div id="cardCollpase31" class="collapse show">
                                                                            <div class="card-body">
                                                                                <div class="form-group">
                                                                                    <label for="HydrologicalRegion">
                                                                                        @if (language == 1)
                                                                                        {<span>i. হাইড্রোলজিক্যাল অঞ্চল অনুযায়ী প্রকল্প এলাকা</span>}
                                                                                        else
                                                                                        {<span>i. Project area according to Hydrological Region</span>}
                                                                                    </label>
                                                                                    <div id="chkHydrologicalRegions" style="margin-left: 5px;">
                                                                                        @{ List<LookUpCcModHydroRegion> _hydro_regions = ViewBag.LookUpCcModHydroRegion;

                                                                                            if (_hydro_regions.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModHydroRegion hr in _hydro_regions)
                                                                                                {
                                                                                                    var id = "chkHydroRegion" + hr.HydroRegionId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullNameBn">
                                                                                                            <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                            <label for="@id"> @hr.HydroRegionFullNameBn </label>
                                                                                                        </div> }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullName">
                                                                                                            <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                            <label for="@id"> @hr.HydroRegionFullName </label>
                                                                                                        </div> }
                                                                                                }
                                                                                            } }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="BDP2100HotSpot">
                                                                                        @if (language == 1)
                                                                                        {<span>ii. বাংলাদেশ ব-দ্বীপ পরিকল্পনা-২১০০ এর হট স্পট অনুযায়ী প্রকল্প এলাকা</span>}
                                                                                        else
                                                                                        {<span>ii. Project Area According to Bangladesh Delta Plan 2100 Hot Spot</span>}
                                                                                    </label>
                                                                                    <div id="chkBDP2100HotSpot" class="row" style="margin-left: 5px;">
                                                                                        @{ List<LookUpCcModDeltPlan2100HotSpot> _hot_spots = ViewBag.LookUpCcModDeltPlan2100HotSpot;

                                                                                            if (_hot_spots.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModDeltPlan2100HotSpot hs in _hot_spots)
                                                                                                {
                                                                                                    var id = "chkBDP2100HotSpot" + hs.DeltaPlanHotSpotId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanNameBn">
                                                                                                            <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                            <label for="@id"> @hs.PlanNameBn </label>
                                                                                                        </div> }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanName">
                                                                                                            <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                            <label for="@id"> @hs.PlanName </label>
                                                                                                        </div> }
                                                                                                }
                                                                                            } }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="NameOfHydraulicStructure">
                                                                                        @if (language == 1)
                                                                                        {<span>iii. হাইড্র্রাউলিক স্ট্রাকচারের নাম</span>}
                                                                                        else
                                                                                        {<span>iii. Name of Hydraulic Structure</span>}
                                                                                    </label>
                                                                                    <input type="text" id="NameOfHydraulicStructure" name="NameOfHydraulicStructure" class="form-control" />
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="TypeOfStructure">
                                                                                        @if (language == 1)
                                                                                        {<span>iv. হাইড্র্রাউলিক স্ট্রাকচারের ধরন</span>}
                                                                                        else
                                                                                        {<span>iv. Type of Hydraulic Structure</span>}
                                                                                    </label>
                                                                                    <input type="text" id="TypeOfStructure" name="TypeOfStructure" class="form-control" />
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="TypeOfTrafficNavigation">
                                                                                        @if (language == 1)
                                                                                        {<span>v. যানবাহন ও নৌচলাচলের ধরন</span>}
                                                                                        else
                                                                                        {<span>v. Type of Traffic and Navigation</span>}
                                                                                    </label>
                                                                                    <input type="text" id="TypeOfTrafficNavigation" name="TypeOfTrafficNavigation" class="form-control" />
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="NavigationClass">
                                                                                        @if (language == 1)
                                                                                        {<span>vi. নৌপথের শ্রেণী</span>}
                                                                                        else
                                                                                        {<span>vi. Navigation Class</span>}
                                                                                    </label>
                                                                                    <div id="NavigationClass" class="row" style="margin-left: 5px;">
                                                                                        @{ List<LookUpCcModNavigationClass> _navc = ViewBag.LookUpCcModNavigationClass;

                                                                                            if (_navc.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModNavigationClass nc in _navc)
                                                                                                {
                                                                                                    var id = "chkNavigationClass" + nc.NavigationClassId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@nc.NavigationClassNameBn">
                                                                                                            <input type="checkbox" name="NavigationClassId" id="@id" value="@nc.NavigationClassId" data-parsley-multiple="NavigationClassId">
                                                                                                            <label for="@id"> @nc.NavigationClassNameBn </label>
                                                                                                        </div> }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@nc.NavigationClassName">
                                                                                                            <input type="checkbox" name="NavigationClassId" id="@id" value="@nc.NavigationClassId" data-parsley-multiple="NavigationClassId">
                                                                                                            <label for="@id"> @nc.NavigationClassName </label>
                                                                                                        </div> }
                                                                                                }
                                                                                            } }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="RiverNature">
                                                                                        @if (language == 1)
                                                                                        {<span>vii. নদীর ধরণ</span>}
                                                                                        else
                                                                                        {<span>vii. River Type</span>}
                                                                                    </label>
                                                                                    <div id="RiverNature" class="row" style="margin-left: 5px;">
                                                                                        @{ List<LookUpCcModRiverType> _rvin = ViewBag.LookUpCcModRiverType;

                                                                                            if (_rvin.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModRiverType rn in _rvin)
                                                                                                {
                                                                                                    var id = "chkRiverType" + rn.RiverTypeId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@rn.RiverTypeNameBn">
                                                                                                            <input type="checkbox" name="RiverTypeId" id="@id" value="@rn.RiverTypeId" data-parsley-multiple="RiverTypeId">
                                                                                                            <label for="@id"> @rn.RiverTypeNameBn </label>
                                                                                                        </div> }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@rn.RiverTypeName">
                                                                                                            <input type="checkbox" name="RiverTypeId" id="@id" value="@rn.RiverTypeId" data-parsley-multiple="RiverTypeId">
                                                                                                            <label for="@id"> @rn.RiverTypeName </label>
                                                                                                        </div> }
                                                                                                }
                                                                                            } }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="RiverNature">
                                                                                        @if (language == 1)
                                                                                        {<span>viii. জোয়ার ভাটার প্রভাব</span>}
                                                                                        else
                                                                                        {<span>viii. River Nature</span>}
                                                                                    </label>
                                                                                    <div id="RiverNature" class="row" style="margin-left: 5px;">
                                                                                        @{ List<LookUpCcModRiverNature> _rvnt = ViewBag.LookUpCcModRiverNature;

                                                                                            if (_rvnt.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModRiverNature rn in _rvnt)
                                                                                                {
                                                                                                    var id = "radRiverNature" + rn.RiverNatureId;

                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="RiverNatureId" id="@id" value="@rn.RiverNatureId" data-parsley-multiple="RiverNatureId">
                                                                                                            <label for="@id"> @rn.RiverNatureTitleBn </label>
                                                                                                        </div> }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="RiverNatureId" id="@id" value="@rn.RiverNatureId" data-parsley-multiple="RiverNatureId">
                                                                                                            <label for="@id"> @rn.RiverNatureTitle </label>
                                                                                                        </div> }
                                                                                                }
                                                                                            } }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="CommandArea">
                                                                                        @if (language == 1)
                                                                                        {<span>ix. কমান্ড এরিয়া (হেক্টর)</span>}
                                                                                        else
                                                                                        {<span>ix. Command Area (ha)</span>}
                                                                                    </label>
                                                                                    <input type="text" id="CommandArea" name="CommandArea" class="form-control" />
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="UsDsParamCondition">
                                                                                        @if (language == 1)
                                                                                        {<span>x. প্রস্তাবিত প্রকল্পের আপস্ট্রিম (Upstream) এবং ডাউনস্ট্রিম (Downstream) এর বর্তমান অবস্থা</span>}
                                                                                        else
                                                                                        {<span>x. U/S and D/S Condition for Proposed Intervention</span>}
                                                                                    </label>

                                                                                    @{
                                                                                        await Html.RenderPartialAsync("controls/_UsDsParamCondition.cshtml",
                                                                                              new ViewDataDictionary(ViewData) {
                                                                                              { "vFormLanguage", language }});
                                                                                    }
                                                                                </div>

                                                                                <div id="divAnnualRainfall">
                                                                                    <label for="AnnualRainfall" class="bold">
                                                                                        @if (language == 1)
                                                                                        {<span>xi. বার্ষিক বৃষ্টিপাত (বিগত পাঁচ বছর) (মি.মি.)</span>}
                                                                                        else
                                                                                        {<span>xi. Annual Rainfall (Last five years) (mm)</span>}
                                                                                    </label>

                                                                                    <hr style="margin-top: 0 !important;" />

                                                                                    <div class="row">
                                                                                        <div class="col-md-12">
                                                                                            @{ await Html.RenderPartialAsync("controls/_AnnualRainfall"); }
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div id="divCrossSectionIntakePoint">
                                                                                    <label for="CrossSectionIntakePoint" class="bold">
                                                                                        @if (language == 1)
                                                                                        {<span>xii. ইনটেক পয়েন্টে প্রস্থছেদ (ক্রস সেকশন) (মিটার)</span>}
                                                                                        else
                                                                                        {<span>xii. Cross Section at Intake Point (m)</span>}
                                                                                    </label>

                                                                                    <hr style="margin-top: 0 !important;" />

                                                                                    <div class="row">
                                                                                        <div class="form-group col-xl-6 text-center">
                                                                                            <label for="CrossSectionDepth">
                                                                                                @if (language == 1)
                                                                                                {<span>গভীরতা</span>}
                                                                                                else
                                                                                                {<span>Depth (m)</span>}
                                                                                            </label>
                                                                                            <input type="number" min="0" id="CrossSectionDepth" name="CrossSectionDepth" class="form-control" />
                                                                                        </div>

                                                                                        <div class="form-group col-md-6 text-center">
                                                                                            <label for="CrossSectionWidth">
                                                                                                @if (language == 1)
                                                                                                {<span>প্রস্থ</span>}
                                                                                                else
                                                                                                {<span>Width (m)</span>}
                                                                                            </label>
                                                                                            <input type="number" min="0" id="CrossSectionWidth" name="CrossSectionWidth" class="form-control" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="WaterLevelInterventionPoint">
                                                                                        @if (language == 1)
                                                                                        {<span>xiii. ইন্টারভনেশন (হাইড্রোলিক অবকাঠামো) পয়েন্টে প্রস্থছেদ (ক্রস সেকশন) (মিটার)</span>}
                                                                                        else
                                                                                        {<span>xiii. Water Level (m PWD) at Intervention Point</span>}
                                                                                    </label>

                                                                                    <div id="WaterLevelInterventionPoint" class="row" style="margin-left: -7px;">
                                                                                        <div class="table-responsive">
                                                                                            <table class="table mb-0">
                                                                                                <thead>
                                                                                                    <tr>
                                                                                                        <th class="text-center">
                                                                                                            @if (language == 1)
                                                                                                            {<span>সর্বোচ্চ</span>}
                                                                                                            else
                                                                                                            {<span>Maximum</span>}
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            @if (language == 1)
                                                                                                            {<span>সর্বনিম্ন</span>}
                                                                                                            else
                                                                                                            {<span>Minimum</span>}
                                                                                                        </th>
                                                                                                    </tr>
                                                                                                </thead>
                                                                                                <tbody>
                                                                                                    <tr>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" min="0" id="WaterLevelMax" name="WaterLevelMax" class="form-control" />
                                                                                                        </td>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" min="0" id="WaterLevelMin" name="WaterLevelMin" class="form-control" />
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                </tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="DischargeInterventionPoint">
                                                                                        @if (language == 1)
                                                                                        {<span>xiv. ইন্টারভনেশন পয়েন্টে পানির সমতল (ওয়াটার লেভেল) (কিউমেক)</span>}
                                                                                        else
                                                                                        {<span>xiv. Discharge (m<sup>3</sup>/s) at Intervention Point</span>}
                                                                                    </label>

                                                                                    <div id="DischargeInterventionPoint" class="row" style="margin-left: -7px;">
                                                                                        <div class="table-responsive">
                                                                                            <table class="table mb-0">
                                                                                                <thead>
                                                                                                    <tr>
                                                                                                        <th class="text-center">
                                                                                                            @if (language == 1)
                                                                                                            {<span>সর্বোচ্চ</span>}
                                                                                                            else
                                                                                                            {<span>Maximum</span>}
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            @if (language == 1)
                                                                                                            {<span>সর্বনিম্ন</span>}
                                                                                                            else
                                                                                                            {<span>Minimum</span>}
                                                                                                        </th>
                                                                                                    </tr>
                                                                                                </thead>
                                                                                                <tbody>
                                                                                                    <tr>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" min="0" id="DischargeMax" name="DischargeMax" class="form-control" />
                                                                                                        </td>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" min="0" id="DischargeMin" name="DischargeMin" class="form-control" />
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                </tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="FloodFrequencyAnalysis">
                                                                                        @if (language == 1)
                                                                                        {<span>xv. ফ্লাড ফ্রিকোয়েন্সি এনালাইসিস; (সর্বোচ্চ পানির উচ্চতা, মিটার)</span>}
                                                                                        else
                                                                                        {<span>xv. Flood Frequency Analysis (Maximum water level in Meter)</span>}
                                                                                    </label>

                                                                                    <div id="FloodFrequencyAnalysis" class="row" style="margin-left: -7px;">
