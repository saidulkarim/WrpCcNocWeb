<!-- Modal Popup for Comments -->
<script type="text/javascript">
    jQuery(document).ready(function () {
        var app_state_id = '@app_state_id';
        var is_complete_id = '@is_complete_id';
        var group_id = '@ViewData["UserLevel"].ToString()';

