            $('.comment').addClass('d-none');
        }

        console.log(app_state_id);

        if (app_state_id == 13 || app_state_id == 23 || app_state_id == 33 || app_state_id == 43) {
            if (is_complete_id < 3) {
                $('.comment').addClass('d-none');
