    var gpwmgtd = ViewData["GPWMGroupType"];

    //List<DataAnalysisControlComments> fdac = (List<DataAnalysisControlComments>)ViewData["FormCommentsListTemp"];

    long ProjectId = (long)ViewData["ProjectId"];
    long ProjectIndvId = (long)ViewData["Project37IndvId"];
    long UserId = (long)ViewData["UserId"];
    int ProjectTypeId = (int)ViewData["ProjectTypeId"];
    var user_group_id = ViewData["UserLevel"].ToString();
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <input id="pProjectId" type="hidden" value="@ProjectId" />
                <input id="pProjectIndvId" type="hidden" value="@ProjectIndvId" />
                <input id="pUserId" type="hidden" value="@UserId" />
                <input id="pProjectTypeId" type="hidden" value="@ProjectTypeId" />

                <center>
                    <div class="col-md-12 text-center">
                        <h3 class="HeaderTitle">
                            @if (prev_lang == "1")
                            {<span>ছাড়পত্রের সনদের জন্য আবেদন</span>}
                            else
                            {<span>Application for Clearance Certificate</span>}
                        </h3>
                        <h4 class="HeaderSubTitle">
                            @if (prev_lang == "1")
                            {<span>(@ViewData["ProjectTypeTitleBn"].ToString())</span>}
                        else
                        {<span>(@ViewData["ProjectTypeTitle"].ToString())</span>}
                        </h4>
                    </div>
                </center>

                <div id="form-preview" class="form-preview container">
                    <div class="row lineGap">
                        <div class="col">
                            <span>
                                @if (prev_lang == "1")
                                {<span>প্রতি</span>}
                                else
                                {<span>To</span>}
