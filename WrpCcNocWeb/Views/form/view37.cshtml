                                                else
                                                {<span>Latitude</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>দ্রাঘিমাংশ</span>}
                                                else
                                                {<span>Longitude</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>সংযুক্তি</span>}
                                                else
                                                {<span>Attchments</span>}
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody id="tbodyProjectLocationPreview">
                                        @foreach (ProjectLocationTemp loc in location as List<ProjectLocationTemp>)
                                        {
                                            if (prev_lang == "1")
                                            {
                                                <tr>
                                                    <td>@loc.UnionNameBn</td>
                                                    <td>@loc.UpazilaNameBn</td>
                                                    <td>@loc.DistrictNameBn</td>
