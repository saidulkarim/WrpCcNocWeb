                                                                                            List<LookUpAdminBndUnion> _unions = ViewBag.LookUpAdminBndUnion;

                                                                                            if (_unions.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpAdminBndUnion uni in _unions)
