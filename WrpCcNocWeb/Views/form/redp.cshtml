@using static WrpCcNocWeb.ViewModels.GeneralInfo
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var language = HttpContextAccessor.HttpContext.Request.Cookies["FormLanguage"].ToString();
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3">
                    <b>@ViewBag.ProjectTitle</b>
                    <button id="btnGoBack" type="button" class="btn btn-secondary float-right" style="margin-top: -10px;">Back</button>
                </h4>

                <div id="basicwizard">
                    <ul class="nav nav-pills bg-light nav-justified form-wizard-header">
                        <li class="nav-item">
                            <a id="lnkTab1" href="#basictab1" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2 active">
                                <i class="mdi mdi-format-float-right mr-1"></i>
                                <span class="d-none d-sm-inline">
                                    @if (language == "1")
                                    {
                                        <span>আবেদন পত্র</span>
                                    }
                                    else
                                    {
                                        <span>Application Form</span>
                                    }
                                </span>
                            </a>
                        </li>
                        @*<li class="nav-item">
                                <a id="lnkTab2" href="#basictab2" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2">
                                    <i class="mdi mdi-face-profile mr-1"></i>
                                    <span class="d-none d-sm-inline">
                                        @if (language == "1")
                                        {
                                            <span>প্রিভিউ</span>
                                        }
                                        else
                                        {
                                            <span>Preview</span>
                                        }
                                    </span>
                                </a>
                            </li>*@
                    </ul>

                    <div class="tab-content b-0 mb-0">
                        <div class="tab-pane active" id="basictab1">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="card-box">
                                        <div class="row">
                                            @Html.Hidden("ProjectId")
                                            @Html.Hidden("AppSubmissionId")
                                            @Html.Hidden("ApplicationStateId")
                                            @Html.Hidden("ApprovalStatusId")
                                            @Html.Hidden("Project39IndvId")
                                            @Html.Hidden("UserId")

                                            <input id="ProjectTypeId" type="hidden" value="@ViewBag.ProjectTypeId" />

                                            <div class="col-sm-3">
                                                @{ Html.RenderPartial("form/_1TabPanel"); }

                                            </div> <!-- end col-->
                                            <div class="col-sm-9">
                                                <div class="tab-content pt-0">
                                                    <div class="tab-pane fade active show" id="v-gen-info" role="tabpanel" aria-labelledby="v-gen-info-tab">
                                                        <form id="frmGeneralInfo">
                                                            <div class="card-box">
                                                                @{ Html.RenderPartial("form/_2GeneralInfo"); }
                                                            </div>
                                                        </form>
                                                    </div>

                                                    <div class="tab-pane fade" id="v-project-location" role="tabpanel" aria-labelledby="v-project-location-tab">
                                                        <div class="card-box">
                                                            @{ Html.RenderPartial("form/_3Location"); }
                                                        </div>
                                                    </div>

                                                    <div class="tab-pane fade" id="v-tech-info" role="tabpanel" aria-labelledby="v-tech-info-tab">
                                                        <form id="frmTechnicalInfo">
                                                            <div class="card-box">
                                                                <div id="tech-info-form">
                                                                    <div class="card">
                                                                        <div class="card-header bg-blue py-3 text-white cursor" data-toggle="collapse" href="#cardCollpase31">
                                                                            <div class="card-widgets">
                                                                                <a data-toggle="collapse" href="#cardCollpase31" role="button" aria-expanded="false" aria-controls="cardCollpase2"><i class="mdi mdi-minus"></i></a>
                                                                            </div>
                                                                            <h5 class="card-title mb-0 text-white">
                                                                                @if (language == "1")
                                                                                {
                                                                                    <span>১. বেজলাইন সম্পর্কিত তথ্য বা অবস্থা পর্যালোচনা</span>}
                                                                                else
                                                                                {
                                                                                    <span>1. Discussion about baseline related information or situation</span>}
                                                                            </h5>
                                                                        </div>
                                                                    </div>
                                                                    <div id="cardCollpase31" class="collapse show">
                                                                        <div class="card-body">
                                                                            <div class="form-group">
                                                                                <label for="HydrologicalRegion">
                                                                                    @if (language == "1")
                                                                                    {<span>i. হাইড্রোলজিক্যাল অঞ্চল</span>}
                                                                                    else
                                                                                    {<span>i. Hydrological Region</span>}
                                                                                </label>
                                                                                <div id="chkHydrologicalRegions" style="margin-left: 5px;">
                                                                                    @{
                                                                                        List<LookUpCcModHydroRegion> _hydro_regions = ViewBag.LookUpCcModHydroRegion;

                                                                                        if (_hydro_regions.Count > 0)
                                                                                        {
                                                                                            foreach (LookUpCcModHydroRegion hr in _hydro_regions)
                                                                                            {
                                                                                                var id = "chkHydroRegion" + hr.HydroRegionId;

                                                                                                if (language == "1")
                                                                                                {
                                                                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullNameBn">
                                                                                                        <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                        <label for="@id"> @hr.HydroRegionFullNameBn </label>
                                                                                                    </div>
                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullName">
                                                                                                        <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                        <label for="@id"> @hr.HydroRegionFullName </label>
                                                                                                    </div>
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="BDP2100HotSpot">
                                                                                    @{if (language == "1")
                                                                                        { <span>ii. বাংলাদেশ ব-দ্বীপ পরিকল্পনা-২১০০  হট স্পট</span>}
                                                                                        else
                                                                                        {<span>ii. Bangladesh Delta Plan 2100 Hot Spot </span>}
                                                                                    }
                                                                                </label>
                                                                                <div id="chkBDP2100HotSpot" class="row" style="margin-left: 5px;">
                                                                                    @{
                                                                                        List<LookUpCcModDeltPlan2100HotSpot> _hot_spots = ViewBag.LookUpCcModDeltPlan2100HotSpot;

                                                                                        if (_hot_spots.Count > 0)
                                                                                        {
                                                                                            foreach (LookUpCcModDeltPlan2100HotSpot hs in _hot_spots)
                                                                                            {
                                                                                                var id = "chkBDP2100HotSpot" + hs.DeltaPlanHotSpotId;

                                                                                                if (language == "1")
                                                                                                {
                                                                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanNameBn">
                                                                                                        <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                        <label for="@id"> @hs.PlanNameBn </label>
                                                                                                    </div>
                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanName">
                                                                                                        <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                        <label for="@id"> @hs.PlanName </label>
                                                                                                    </div>
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="NameOfDredgedRiver">
                                                                                    @{if (language == "1")
                                                                                        { <span>iii. ড্রেজিংকৃতব্য নদীর নাম</span>}
                                                                                        else
                                                                                        {<span>iii. Name of the dredged River</span>}
                                                                                    }
                                                                                </label>
                                                                                <input type="text" id="NameOfDredgedRiver" name="NameOfDredgedRiver" class="form-control" />
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="RiverNature">
                                                                                    @{if (language == "1")
                                                                                        { <span>iv. নদীর ধরণ</span> }
                                                                                        else
                                                                                        { <span>iv. River Nature</span> }
                                                                                    }
                                                                                </label>
                                                                                <div id="RiverNature" class="row" style="margin-left: 5px;">
                                                                                    @{
                                                                                        List<LookUpCcModRiverNature> _rvnt = ViewBag.LookUpCcModRiverNature;

                                                                                        if (_rvnt.Count > 0)
                                                                                        {
                                                                                            foreach (LookUpCcModRiverNature rn in _rvnt)
                                                                                            {
                                                                                                var id = "radRiverNature" + rn.RiverNatureId;

                                                                                                if (language == "1")
                                                                                                {
                                                                                                    <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                        <input type="radio" name="RiverNatureId" id="@id" value="@rn.RiverNatureId" data-parsley-multiple="RiverNatureId">
                                                                                                        <label for="@id"> @rn.RiverNatureTitleBn </label>
                                                                                                    </div>
                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                        <input type="radio" name="RiverNatureId" id="@id" value="@rn.RiverNatureId" data-parsley-multiple="RiverNatureId">
                                                                                                        <label for="@id"> @rn.RiverNatureTitle </label>
                                                                                                    </div>
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="RiverSystem">
                                                                                    @{if (language == "1")
                                                                                        { <span>v. নদীর নিয়ম বদ্ধ রীতি (খননকৃত নদীর সাথে নদীটির আন্তঃ সংযোগ স্থাপন)</span>}
                                                                                        else
                                                                                        {<span>v. River system (inter linked river with dredged river)</span>}
                                                                                    }
                                                                                </label>
                                                                                <input type="text" id="RiverSystem" name="RiverSystem" class="form-control" />
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="UsDsParamCondition">
                                                                                    @{if (language == "1")
                                                                                        { <span>vi. প্রস্তাবিত প্রকল্পের আপস্ট্রিম এবং ডাউনস্ট্রিম এর বর্তমান অবস্থাব</span>}
                                                                                        else
                                                                                        {<span>vi. U/S and D/S Condition for Proposed Intervention</span>}
                                                                                    }
                                                                                </label>

                                                                                <div id="UsDsParamCondition" class="row" style="margin-left: -7px;">
                                                                                    <div class="table-responsive">
                                                                                        <table class="table mb-0">
                                                                                            <thead>
                                                                                                <tr>
                                                                                                    <th class="text-center">#</th>
                                                                                                    <th class="text-center">
                                                                                                        @if (language == "1")
                                                                                                        {<span>বিবরন</span> }
                                                                                                        else
                                                                                                        { <span>Parameter</span>}
                                                                                                    </th>
                                                                                                    <th class="text-center">
                                                                                                        <span id="HydroSystemName"></span>
                                                                                                        @if (language == "1")
                                                                                                        {<span>আপস্ট্রিম</span> }
                                                                                                        else
                                                                                                        { <span>Upstream</span>}
                                                                                                    </th>
                                                                                                    <th class="text-center">
                                                                                                        @if (language == "1")
                                                                                                        {<span>ডাউনস্ট্রিম</span> }
                                                                                                        else
                                                                                                        { <span>Downstream</span>}
                                                                                                    </th>
                                                                                                    <th class="text-center"></th>
                                                                                                </tr>
                                                                                                <tr>
                                                                                                    <th colspan="2" class="text-center">
                                                                                                        <input id="UsDsConditionDetailId" type="hidden" />
                                                                                                        <select id="UsDsConditionId" name="UsDsConditionId" class="form-control">
                                                                                                            <option value="">
                                                                                                                @{if (language == "1")
                                                                                                                    { <span>বাছাই করুন...</span>}
                                                                                                                    else
                                                                                                                    {<span>Choose...</span>}
                                                                                                                }
                                                                                                            </option>
                                                                                                            @{
                                                                                                                List<LookUpCcModUsDsCondition> _ud = ViewBag.LookUpCcModUsDsCondition;

                                                                                                                if (_ud.Count > 0)
                                                                                                                {
                                                                                                                    foreach (LookUpCcModUsDsCondition ud in _ud)
                                                                                                                    {
                                                                                                                        if (language == "1")
                                                                                                                        {
                                                                                                                            <option value="@ud.UsDsConditionId" title="@ud.ParameterNameBn">@ud.ParameterNameBn</option>
                                                                                                                        }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option value="@ud.UsDsConditionId" title="@ud.ParameterName">@ud.ParameterName</option>
                                                                                                                        }
                                                                                                                    }
                                                                                                                }
                                                                                                            }
                                                                                                        </select>
                                                                                                    </th>
                                                                                                    <th class="text-center">
                                                                                                        <input type="text" id="UsParameterValue" name="UsParameterValue" class="form-control" />
                                                                                                    </th>
                                                                                                    <th class="text-center">
                                                                                                        <input type="text" id="DsParameterValue" name="DsParameterValue" class="form-control" />
                                                                                                    </th>
                                                                                                    <th class="text-center">
                                                                                                        <button id="btnSaveUsDsParams" type="button" class="btn btn-success waves-effect waves-light" value="Save">
                                                                                                            <i class="mdi mdi-content-save"></i>
                                                                                                        </button>

                                                                                                        <button id="btnClearUsDsParams" type="button" class="btn btn-warning waves-effect waves-light" value="Clear">
                                                                                                            <i class="mdi mdi-refresh"></i>
                                                                                                        </button>
                                                                                                    </th>
                                                                                                </tr>
                                                                                            </thead>
                                                                                            <tbody id="tbodyUsDsParamCondition"></tbody>
                                                                                        </table>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="CatchmentArea">
                                                                                    @{if (language == "1")
                                                                                        { <span>vii. অববাহিকা (ক্যাচমেন্ট) এলাকা (হেক্টর)</span>}
                                                                                        else
                                                                                        {<span>vii. Catchment Area (ha)</span>}
                                                                                    }
                                                                                </label>
                                                                                <input type="number" id="CatchmentArea" name="CatchmentArea" class="form-control" />
                                                                            </div>

                                                                            <div id="divAnnualRainfall">
                                                                                <label for="AnnualRainfall" class="bold">
                                                                                    @{if (language == "1")
                                                                                        { <span>viii. বার্ষিক বৃষ্টিপাত (বিগত পাঁচ বছর) (মি.মি.)</span>}
                                                                                        else
                                                                                        {<span>viii. Annual Rainfall (last five years)</span>}
                                                                                    }
                                                                                </label>

                                                                                <hr style="margin-top: 0 !important;" />

                                                                                <div class="row">
                                                                                    <div class="form-group col-xl-3 text-center">
                                                                                        @{
                                                                                            int year1 = DateTime.Now.Year - 1;

                                                                                            <label for="AnnualRainFallLast1Year">@year1</label>
                                                                                            <input type="number" id="AnnualRainFallLast1Year" name="AnnualRainFallLast1Year" class="form-control" />
                                                                                        }
                                                                                    </div>

                                                                                    <div class="form-group col-md-2 text-center">
                                                                                        @{
                                                                                            int year2 = DateTime.Now.Year - 2;
                                                                                            <label for="AnnualRainFallLast2Years">@year2</label>
                                                                                            <input type="number" id="AnnualRainFallLast2Years" name="AnnualRainFallLast2Years" class="form-control" />
                                                                                        }
                                                                                    </div>

                                                                                    <div class="form-group col-xl-2 text-center">
                                                                                        @{
                                                                                            int year3 = DateTime.Now.Year - 3;
                                                                                            <label for="AnnualRainFallLast3Years">@year3</label>
                                                                                            <input type="number" id="AnnualRainFallLast3Years" name="AnnualRainFallLast3Years" class="form-control" />
                                                                                        }
                                                                                    </div>

                                                                                    <div class="form-group col-md-2 text-center">
                                                                                        @{
                                                                                            int year4 = DateTime.Now.Year - 4;
                                                                                            <label for="AnnualRainFallLast4Years">@year4</label>
                                                                                            <input type="number" id="AnnualRainFallLast4Years" name="AnnualRainFallLast4Years" class="form-control" />
                                                                                        }
                                                                                    </div>

                                                                                    <div class="form-group col-xl-3 text-center">
                                                                                        @{
                                                                                            int year5 = DateTime.Now.Year - 5;
                                                                                            <label for="AnnualRainFallLast5Years">@year5</label>
                                                                                            <input type="number" id="AnnualRainFallLast5Years" name="AnnualRainFallLast5Years" class="form-control" />
                                                                                        }
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="DrainageCondition">
                                                                                    @{if (language == "1")
                                                                                        { <span>ix. নিষ্কাশন (ড্রেইনেজ) ব্যবস্থা</span>}
                                                                                        else
                                                                                        {<span>ix. Drainage Condition</span>}
                                                                                    }
                                                                                </label>
                                                                                <div id="DrainageCondition" class="row" style="margin-left: 5px;">
                                                                                    @{
                                                                                        List<LookUpCcModDrainageCondition> _dcs = ViewBag.LookUpCcModDrainageCondition;

                                                                                        if (_dcs.Count > 0)
                                                                                        {
                                                                                            foreach (LookUpCcModDrainageCondition dc in _dcs)
                                                                                            {
                                                                                                var id = "chkDrainageCondition" + dc.DrainageConditionId;

                                                                                                if (language == "1")
                                                                                                {
                                                                                                    <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                        <input type="radio" name="DrainageConditionId" id="@id" value="@dc.DrainageConditionId" data-parsley-multiple="DrainageConditionId">
                                                                                                        <label for="@id"> @dc.DrainageConditionBn </label>
                                                                                                    </div>
                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                        <input type="radio" name="DrainageConditionId" id="@id" value="@dc.DrainageConditionId" data-parsley-multiple="DrainageConditionId">
                                                                                                        <label for="@id"> @dc.DrainageCondition </label>
                                                                                                    </div>
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="LengthDredgingWork">
                                                                                    @{if (language == "1")
                                                                                        { <span>x. ড্রেজিংকৃত্যব কাজের দৈর্ঘ্য/চেইনেজসহ ড্রেজিং</span>}
                                                                                        else
                                                                                        {<span>x. Length of Dredging Work/ Dredging Segment with Chainage (KM)</span>}
                                                                                    }
                                                                                </label>
                                                                                <input type="number" id="LengthDredgingWork" name="LengthDredgingWork" class="form-control" />
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="NavigationClass">
                                                                                    @{if (language == "1")
                                                                                        {<span>xi. নৌপথের শ্রেণী</span>}
                                                                                        else
                                                                                        {<span>xi. Navigation Class</span>}
                                                                                    }
                                                                                </label>
                                                                                <div id="NavigationClass" class="row" style="margin-left: 5px;">
                                                                                    @{
                                                                                        List<LookUpCcModNavigationClass> _navc = ViewBag.LookUpCcModNavigationClass;

                                                                                        if (_navc.Count > 0)
                                                                                        {
                                                                                            foreach (LookUpCcModNavigationClass nc in _navc)
                                                                                            {
                                                                                                var id = "chkNavigationClass" + nc.NavigationClassId;

                                                                                                if (language == "1")
                                                                                                {
                                                                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@nc.NavigationClassNameBn">
                                                                                                        <input type="checkbox" name="NavigationClassId" id="@id" value="@nc.NavigationClassId" data-parsley-multiple="NavigationClassId">
                                                                                                        <label for="@id"> @nc.NavigationClassNameBn </label>
                                                                                                    </div>
                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@nc.NavigationClassName">
                                                                                                        <input type="checkbox" name="NavigationClassId" id="@id" value="@nc.NavigationClassId" data-parsley-multiple="NavigationClassId">
                                                                                                        <label for="@id"> @nc.NavigationClassName </label>
                                                                                                    </div>
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </div>
                                                                            </div>

                                                                            <div id="divFishHabitat">
                                                                                <label for="FishHabitat" class="bold">
                                                                                    @{if (language == "1")
                                                                                        { <span>xii. মৎস প্রতিপালন</span>}
                                                                                        else
                                                                                        {<span>xii. Fish Habitat</span>}
                                                                                    }
                                                                                </label>

                                                                                <hr style="margin-top: 0 !important;" />

                                                                                <div class="row">
                                                                                    <div class="form-group col-xl-6 text-center">
                                                                                        <label for="FishHabitatArea">
                                                                                            @if (language == "1")
                                                                                            {<span>এরিয়া (হেক্টর)</span> }
                                                                                            else
                                                                                            { <span>Area (ha)</span>}
                                                                                        </label>
                                                                                        <input type="number" id="FishHabitatArea" name="FishHabitatArea" class="form-control" />
                                                                                    </div>

                                                                                    <div class="form-group col-md-6 text-center">
                                                                                        <label for="FishHabitatProduction">
                                                                                            @if (language == "1")
                                                                                            {<span>উৎপাদন (টন)</span> }
                                                                                            else
