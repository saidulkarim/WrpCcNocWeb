                                                                                                        <input type="radio" name="RiverNatureId" id="@id" value="@rn.WaterBodyTypeId" data-parsley-multiple="RiverNatureId">
                                                                                                        <label for="@id"> @rn.WaterBodyTypeBn </label>
                                                                                                    </div>
                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                        <input type="radio" name="RiverNatureId" id="@id" value="@rn.WaterBodyTypeId" data-parsley-multiple="RiverNatureId">
                                                                                                        <label for="@id"> @rn.WaterBodyType </label>
                                                                                                    </div>
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    }
                                                                                </div>
                                                                            </div>

                                                                            <div id="divBankErosion">
                                                                                <label for="BankErosion" class="bold">
                                                                                    @if (language == 1)
                                                                                    {<span>v. নদী ভাঙ্গন</span>}
