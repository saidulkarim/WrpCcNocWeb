@using static WrpCcNocWeb.ViewModels.GeneralInfo
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{    
    var language = HttpContextAccessor.HttpContext.Request.Cookies["FormLanguage"].ToString();
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3"> Apply for: <b>@ViewBag.ProjectTitle</b></h4>
                <form>
                    <div id="basicwizard">
                        <ul class="nav nav-pills bg-light nav-justified form-wizard-header">
                            <li class="nav-item">
                                <a id="lnkTab1" href="#basictab1" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2 active">
                                    <i class="mdi mdi-format-float-right mr-1"></i>
                                    <span class="d-none d-sm-inline">@ViewBag.ProjectTitle</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a id="lnkTab2" href="#basictab2" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2">
                                    <i class="mdi mdi-face-profile mr-1"></i>
                                    <span class="d-none d-sm-inline">Preview</span>
                                </a>
                            </li>
                        </ul>

                        <div class="tab-content b-0 mb-0">
                            <div class="tab-pane active" id="basictab1">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="card-box">
                                            <div class="row">
                                                @Html.Hidden("ProjectId")
                                                @Html.Hidden("AppSubmissionId")
                                                @Html.Hidden("ApplicationStateId")
                                                @Html.Hidden("ApprovalStatusId")
                                                @Html.Hidden("Project31IndvId")
                                                @Html.Hidden("UserId")

                                                <input id="ProjectTypeId" type="hidden" value="@ViewBag.ProjectTypeId" />

                                                <div class="col-sm-3">
                                                    <div class="nav flex-column nav-pills nav-pills-tab" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                                                        <a class="nav-link show mb-2 active" id="v-gen-info-tab" data-toggle="pill" href="#v-gen-info" role="tab" aria-controls="v-gen-info" aria-selected="true">
                                                            @{if (language == "1")
                                                                { <span> সাধারণ তথ্য </span>}
                                                                else
                                                                {<span> General Information </span>}
                                                            }
                                                        </a>
                                                        <a class="nav-link mb-2" id="v-project-location-tab" data-toggle="pill" href="#v-project-location" role="tab" aria-controls="v-project-location" aria-selected="false">
                                                            Project Location
                                                        </a>
                                                        <a class="nav-link mb-2" id="v-tech-info-tab" data-toggle="pill" href="#v-tech-info" role="tab" aria-controls="v-tech-info" aria-selected="false">
                                                            Technical Information
                                                        </a>
                                                        <a class="nav-link mb-2" id="v-deed-obligatory-tab" data-toggle="pill" href="#v-deed-obligatory" role="tab" aria-controls="v-deed-obligatory" aria-selected="false">
                                                            Deed Obligatory
                                                        </a>
                                                        <a class="nav-link mb-2" id="v-administrative-tab" data-toggle="pill" href="#v-administrative" role="tab" aria-controls="v-administrative" aria-selected="false">
                                                            Administrative
                                                        </a>
                                                        <a class="nav-link mb-2" id="v-final-submit-tab" data-toggle="pill" href="#v-final-submit" role="tab" aria-controls="v-final-submit" aria-selected="false">
                                                            Final Submit
                                                        </a>
                                                    </div>
                                                </div> <!-- end col-->
                                                <div class="col-sm-9">
                                                    <div class="tab-content pt-0">
                                                        <div class="tab-pane fade active show" id="v-gen-info" role="tabpanel" aria-labelledby="v-gen-info-tab">
                                                            <div class="card-box">
                                                                <div id="gen-info-form">
                                                                    <div class="form-group">
                                                                        <label for="ProjectName">Project Name <span class="text-danger bold">*</span></label>
                                                                        <input type="text" id="ProjectName" name="ProjectName" class="form-control" required="" />
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="BackgroundAndRationale">Project Background and Rationale <span class="text-danger bold">*</span></label>
                                                                        <textarea id="BackgroundAndRationale" name="BackgroundAndRationale" class="form-control" rows="2" required=""></textarea>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="ProjectTarget">Project Target <span class="text-danger bold">*</span></label>
                                                                        <textarea id="ProjectTarget" name="ProjectTarget" class="form-control" rows="2" required=""></textarea>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="ProjectObjective">Project Objective <span class="text-danger bold">*</span></label>
                                                                        <textarea id="ProjectObjective" name="ProjectObjective" class="form-control" rows="2" required=""></textarea>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="ProjectActivity">Project Activities/ Components</label>
                                                                        <textarea id="ProjectActivity" name="ProjectActivity" class="form-control" rows="2" required=""></textarea>
                                                                    </div>

                                                                    <div class="divProjectPeriod">
                                                                        <fieldset>
                                                                            <legend>
                                                                                <label for="ProjectPeriod" class="bold">Project Period</label>
                                                                            </legend>

                                                                            <div class="row">
                                                                                <div class="form-group col-md-6">
                                                                                    <label for="ProjectStartDate">Starting Time</label>
                                                                                    <input type="text" id="ProjectStartDate" name="ProjectStartDate" class="form-control datetimepicker" />
                                                                                </div>

                                                                                <div class="form-group col-md-6">
                                                                                    <label for="ProjectCompletionDate">Completion Time</label>
                                                                                    <input type="text" id="ProjectCompletionDate" name="ProjectCompletionDate" class="form-control datetimepicker" />
                                                                                </div>
                                                                            </div>
                                                                        </fieldset>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="ProjectEstimatedCost">Estimated Project Cost</label>
                                                                        <textarea id="ProjectEstimatedCost" name="ProjectEstimatedCost" class="form-control" rows="3" required=""></textarea>
                                                                    </div>

                                                                    <div class="form-group mb-0">
                                                                        <button id="btnSaveGeneral" type="button" class="btn btn-info waves-effect waves-light">
                                                                            <span class="btn-label"><i class="mdi mdi-content-save"></i></span>
                                                                            <span style="text-decoration: underline;">S</span>ave
                                                                            <span class="myspinner spinner-border spinner-border-sm mr-1 d-none"></span>
                                                                        </button>

                                                                        <button id="btnClearGeneral" type="button" class="btn btn-secondary waves-effect waves-light">
                                                                            <span class="btn-label"><i class="mdi mdi-close-outline"></i></span>
                                                                            Clear
                                                                        </button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="tab-pane fade" id="v-project-location" role="tabpanel" aria-labelledby="v-project-location-tab">
                                                            <div class="card-box">
                                                                <div id="proj-loc-form">
                                                                    <div id="divProjectLocation">
                                                                        <div id="ProjectLocation" class="row" style="margin-left: -7px;">
                                                                            <div class="table-responsive">
                                                                                <table class="table mb-0">
                                                                                    <thead>
                                                                                        <tr>
                                                                                            <th class="text-center">#</th>
                                                                                            <th class="text-center">District</th>
                                                                                            <th class="text-center">Upazila</th>
                                                                                            <th class="text-center">Union</th>
                                                                                            <th class="text-center">Latitude</th>
                                                                                            <th class="text-center">Longitude</th>
                                                                                            <th class="text-center">Attachment</th>
                                                                                            <th class="text-center" style="width: 150px;"></th>
                                                                                        </tr>
                                                                                        <tr>
                                                                                            <th colspan="2" class="text-center">
                                                                                                <input id="LocationId" type="hidden" />

                                                                                                <select id="DistrictGeoCode" name="DistrictGeoCode" class="form-control" required="">
                                                                                                    <option value="">Choose..</option>

                                                                                                    @{
                                                                                                        List<LookUpAdminBndDistrict> _districts = ViewBag.LookUpAdminBndDistrict;

                                                                                                        if (_districts.Count > 0)
                                                                                                        {
                                                                                                            foreach (LookUpAdminBndDistrict dist in _districts)
                                                                                                            {
                                                                                                                <option value="@dist.DistrictGeoCode">@dist.DistrictName</option>
                                                                                                            }
                                                                                                        }
                                                                                                    }
                                                                                                </select>
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <select id="UpazilaGeoCode" name="UpazilaGeoCode" class="form-control">
                                                                                                    <option value="">Choose..</option>
                                                                                                </select>
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <select id="UnionGeoCode" name="UnionGeoCode" class="form-control">
                                                                                                    <option value="">Choose..</option>
                                                                                                </select>
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <input type="number" id="Latitude" name="Latitude" class="form-control text-right" />
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <input type="number" id="Longitude" name="Longitude" class="form-control text-right" />
                                                                                            </th>
                                                                                            <th class="text-center" style="vertical-align: central;">
                                                                                                <input type="file" id="ImageFileName" name="ImageFileName" class="form-control-file">
                                                                                                <!--<input id="files" name="files" type="file" multiple onchange="uploadFiles('files');" />-->
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <button id="btnSaveProjLocation" type="button" class="btn btn-success waves-effect waves-light" value="Save">
                                                                                                    <i class="mdi mdi-content-save"></i>
                                                                                                </button>

                                                                                                <button id="btnClearProjLocation" type="button" class="btn btn-warning waves-effect waves-light" value="Clear">
                                                                                                    <i class="mdi mdi-refresh"></i>
                                                                                                </button>
                                                                                            </th>
                                                                                        </tr>
                                                                                    </thead>
                                                                                    <tbody id="tbodyProjectLocation"></tbody>
                                                                                </table>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="tab-pane fade" id="v-tech-info" role="tabpanel" aria-labelledby="v-tech-info-tab">
                                                            <div class="card-box">
                                                                <div id="tech-info-form">
                                                                    <h2>1.	Discussion about baseline related information or situation</h2>

                                                                    <div class="form-group">
                                                                        <label for="HydrologicalRegion">Hydrological Region</label>
                                                                        <div id="chkHydrologicalRegions" style="margin-left: 5px;">
                                                                            @{
                                                                                List<LookUpCcModHydroRegion> _hydro_regions = ViewBag.LookUpCcModHydroRegion;

                                                                                if (_hydro_regions.Count > 0)
                                                                                {
                                                                                    foreach (LookUpCcModHydroRegion hr in _hydro_regions)
                                                                                    {
                                                                                        var id = "chkHydroRegion" + hr.HydroRegionId;
                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullName">
                                                                                            <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                            <label for="@id"> @hr.HydroRegionShortName </label>
                                                                                        </div>
                                                                                    }
                                                                                }
                                                                            }
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="BDP2100HotSpot">Bangladesh Delta Plan 2100 Hot Spot</label>
                                                                        <div id="chkBDP2100HotSpot" class="row" style="margin-left: 5px;">
                                                                            @{
                                                                                List<LookUpCcModDeltPlan2100HotSpot> _hot_spots = ViewBag.LookUpCcModDeltPlan2100HotSpot;

                                                                                if (_hot_spots.Count > 0)
                                                                                {
                                                                                    foreach (LookUpCcModDeltPlan2100HotSpot hs in _hot_spots)
                                                                                    {
                                                                                        var id = "chkBDP2100HotSpot" + hs.DeltaPlanHotSpotId;
                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" title="@hs.PlanName">
                                                                                            <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                            <label for="@id"> @hs.PlanName </label>
                                                                                        </div>
                                                                                    }
                                                                                }
                                                                            }
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="HydrologicalSystem">Hydrological System</label>

                                                                        <div id="HydrologicalSystem" class="row" style="margin-left: -7px;">
                                                                            <div class="table-responsive">
                                                                                <table class="table mb-0">
                                                                                    <thead>
                                                                                        <tr>
                                                                                            <th class="text-center">#</th>
                                                                                            <th class="text-center">Hydrological System</th>
                                                                                            <th class="text-center"><span id="HydroSystemName"></span> Name</th>
                                                                                            <th class="text-center">Length or Area</th>
                                                                                            <th class="text-center"></th>
                                                                                        </tr>
                                                                                        <tr>
                                                                                            <th colspan="2" class="text-center">
                                                                                                <input id="HydroSysDetailId" type="hidden" />
                                                                                                <select id="HydroSystemCategoryId" name="HydroSystemCategoryId" class="form-control">
                                                                                                    <option value="">Choose...</option>
                                                                                                    @{
                                                                                                        List<LookUpCcModHydroSystem> _hs = ViewBag.LookUpCcModHydroSystem;

                                                                                                        if (_hs.Count > 0)
                                                                                                        {
                                                                                                            foreach (LookUpCcModHydroSystem hs in _hs)
                                                                                                            {
                                                                                                                <option value="@hs.HydroSystemCategoryId" title="@hs.HydroSysCategoryUnit">@hs.HydroSystemCategory</option>
                                                                                                            }
                                                                                                        }
                                                                                                    }
                                                                                                </select>
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <input type="text" id="NameOfHydroSystem" name="NameOfHydroSystem" class="form-control" />
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <div class="input-group">
                                                                                                    <input type="number" id="HydroSystemLengthArea" name="HydroSystemLengthArea" class="form-control" />
                                                                                                    <div class="input-group-append d-none">
                                                                                                        <span id="HydroSysCategoryUnit" class="input-group-text">Unit</span>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <button id="btnSaveHydroSystem" type="button" class="btn btn-success waves-effect waves-light" value="Save">
                                                                                                    <i class="mdi mdi-content-save"></i>
                                                                                                </button>

                                                                                                <button id="btnClearHydroSystem" type="button" class="btn btn-warning waves-effect waves-light" value="Clear">
                                                                                                    <i class="mdi mdi-refresh"></i>
                                                                                                </button>
                                                                                            </th>
                                                                                        </tr>
                                                                                    </thead>
                                                                                    <tbody id="tbodyHydrologicalSystem"></tbody>
                                                                                </table>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="ConnectivityAmidWaterland">Connectivity Amid Khals, River and Wetland</label>
                                                                        <input type="text" id="ConnectivityAmidWaterland" name="ConnectivityAmidWaterland" class="form-control" />
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="CatchmentArea">Catchment Area (ha)</label>
                                                                        <input type="text" id="CatchmentArea" name="CatchmentArea" class="form-control" />
                                                                    </div>

                                                                    <div id="divAnnualRainfall">
                                                                        <label for="AnnualRainfall" class="bold">Annual Rainfall (last five years)</label>

                                                                        <hr style="margin-top: 0 !important;" />

                                                                        <div class="row">
                                                                            <div class="form-group col-xl-3 text-center">
                                                                                @{
                                                                                    int year1 = DateTime.Now.Year - 1;

                                                                                    <label for="AnnualRainFallLast1Year">@year1</label>
                                                                                    <input type="text" id="AnnualRainFallLast1Year" name="AnnualRainFallLast1Year" class="form-control" />
                                                                                }
                                                                            </div>

                                                                            <div class="form-group col-md-2 text-center">
                                                                                @{
                                                                                    int year2 = DateTime.Now.Year - 2;
                                                                                    <label for="AnnualRainFallLast2Years">@year2</label>
                                                                                    <input type="text" id="AnnualRainFallLast2Years" name="AnnualRainFallLast2Years" class="form-control" />
                                                                                }
                                                                            </div>

                                                                            <div class="form-group col-xl-2 text-center">
                                                                                @{
                                                                                    int year3 = DateTime.Now.Year - 3;
                                                                                    <label for="AnnualRainFallLast3Years">@year3</label>
                                                                                    <input type="text" id="AnnualRainFallLast3Years" name="AnnualRainFallLast3Years" class="form-control" />
                                                                                }
                                                                            </div>

                                                                            <div class="form-group col-md-2 text-center">
                                                                                @{
                                                                                    int year4 = DateTime.Now.Year - 4;
                                                                                    <label for="AnnualRainFallLast4Years">@year4</label>
                                                                                    <input type="text" id="AnnualRainFallLast4Years" name="AnnualRainFallLast4Years" class="form-control" />
                                                                                }
                                                                            </div>

                                                                            <div class="form-group col-xl-3 text-center">
                                                                                @{
                                                                                    int year5 = DateTime.Now.Year - 5;
                                                                                    <label for="AnnualRainFallLast5Years">@year5</label>
                                                                                    <input type="text" id="AnnualRainFallLast5Years" name="AnnualRainFallLast5Years" class="form-control" />
                                                                                }
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="HighestFloodLevel">Highest Flood Level (m PWD)</label>
                                                                        <input type="text" id="HighestFloodLevel" name="HighestFloodLevel" class="form-control" />
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="MaximumDischarge">Maximum Discharge (m<sup>3</sup>/s)</label>
                                                                        <input type="text" id="MaximumDischarge" name="MaximumDischarge" class="form-control" />
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="TypesOfFlood">Types of Flood</label>

                                                                        <div id="TypesOfFlood" class="row" style="margin-left: 5px;">
                                                                            @{
                                                                                List<LookUpCcModTypeOfFlood> _tofs = ViewBag.LookUpCcModTypeOfFlood;

                                                                                if (_tofs.Count > 0)
                                                                                {
                                                                                    foreach (LookUpCcModTypeOfFlood tof in _tofs)
                                                                                    {
                                                                                        var id = "chkTypeOfFlood" + tof.FloodTypeId;
                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 5%;" title="@tof.FloodTypeName">
                                                                                            <input type="checkbox" name="types_of_flood" id="@id" value="@tof.FloodTypeId" data-parsley-multiple="types_of_flood">
                                                                                            <label for="@id"> @tof.FloodTypeName </label>
                                                                                        </div>
                                                                                    }
                                                                                }
                                                                            }
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="FloodFrequencyAnalysis">Flood Frequency Analysis (Maximum water level in m, PWD)</label>

                                                                        <div id="FloodFrequencyAnalysis" class="row" style="margin-left: -7px;">
                                                                            <div class="table-responsive">
                                                                                <table class="table mb-0">
                                                                                    <thead>
                                                                                        <tr>
                                                                                            <th class="text-center">#</th>
                                                                                            <th class="text-center">Flood Frequency</th>
                                                                                            <th class="text-center">Frequency</th>
                                                                                            <th class="text-center"></th>
                                                                                        </tr>
                                                                                        <tr>
                                                                                            <th colspan="2" class="text-center">
                                                                                                <input id="FloodFrequencyDetailId" type="hidden" />
                                                                                                <select id="FloodFrequencyId" name="FloodFrequencyId" class="form-control">
                                                                                                    <option value="">Choose..</option>
                                                                                                    @{
                                                                                                        List<LookUpCcModFloodFrequency> _floodfrequency = ViewBag.FloodFrequencyId;

                                                                                                        if (_floodfrequency.Count > 0)
                                                                                                        {
                                                                                                            foreach (LookUpCcModFloodFrequency flood in _floodfrequency)
                                                                                                            {
                                                                                                                <option value="@flood.FloodFrequencyId">@flood.FloodFrequency</option>
                                                                                                            }
                                                                                                        }
                                                                                                    }
                                                                                                </select>
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <input type="text" id="FloodFrequencyLevel" name="FloodFrequencyLevel" class="form-control" />
                                                                                            </th>
                                                                                            <th class="text-center">
                                                                                                <button id="btnSaveFloodFrequency" type="button" class="btn btn-success waves-effect waves-light" value="Save">
                                                                                                    <i class="mdi mdi-content-save"></i>
                                                                                                </button>

                                                                                                <button id="btnClearFloodFrequency" type="button" class="btn btn-warning waves-effect waves-light" value="Save">
                                                                                                    <i class="mdi mdi-refresh"></i>
                                                                                                </button>
                                                                                            </th>
                                                                                        </tr>
                                                                                    </thead>
                                                                                    <tbody id="tbodyFloodFrequency"></tbody>
                                                                                </table>
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group">
                                                                        <label for="DrainageCondition">Drainage Condition</label>
                                                                        <div id="DrainageCondition" class="row" style="margin-left: 5px;">
                                                                            @{
                                                                                List<LookUpCcModDrainageCondition> _dcs = ViewBag.LookUpCcModDrainageCondition;

                                                                                if (_dcs.Count > 0)
                                                                                {
                                                                                    int i = 1;
                                                                                    foreach (LookUpCcModDrainageCondition dc in _dcs)
                                                                                    {
                                                                                        var id = "chkDrainageCondition" + dc.DrainageConditionId;
                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                            <input type="radio" name="drainage_condition" id="@id" value="@dc.DrainageConditionId" data-parsley-multiple="drainage_condition">
                                                                                            <label for="@id"> @dc.DrainageCondition </label>
                                                                                        </div>
                                                                                    }
                                                                                }
                                                                            }
                                                                        </div>
                                                                    </div>

                                                                    <div id="divSurfaceWaterQuality">
                                                                        <label for="SurfaceWaterQuality" class="bold">Surface Water Quality</label>

                                                                        <hr style="margin-top: 0 !important;" />

                                                                        <div class="row">
                                                                            <div class="form-group col-xl-3 text-center">
                                                                                <label for="WaterSalinity">Salinity (ppm)</label>
                                                                                <input type="text" id="WaterSalinity" name="WaterSalinity" class="form-control" />
                                                                            </div>

                                                                            <div class="form-group col-md-3 text-center">
                                                                                <label for="WaterDO">DO (mg/l)</label>
                                                                                <input type="text" id="WaterDO" name="WaterDO" class="form-control" />
                                                                            </div>

                                                                            <div class="form-group col-xl-3 text-center">
                                                                                <label for="WaterTDS">TDS</label>
                                                                                <input type="text" id="WaterTDS" name="WaterTDS" class="form-control" />
                                                                            </div>

                                                                            <div class="form-group col-md-3 text-center">
                                                                                <label for="WaterPhLevel">pH</label>
                                                                                <input type="text" id="WaterPhLevel" name="WaterPhLevel" class="form-control" />
                                                                            </div>
                                                                        </div>
                                                                    </div>

                                                                    <div id="divSurfaceWaterQuality">
                                                                        <label for="LandType" class="bold">Land Type (Percent)</label>

                                                                        <hr style="margin-top: 0 !important;" />

