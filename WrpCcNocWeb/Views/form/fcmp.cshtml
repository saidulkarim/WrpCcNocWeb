        function GetProjLocationsData() {
            var ProjectId = GetHiddenValue('ProjectId');
            var url = '@Url.Action("get_pld", "form")';

            jQuery.ajax({
                type: "GET",
                url: url,
                data: { project_id: ProjectId },
                dataType: "json",
                success: function (data, status, jqXHR) {
                    if (data.length > 0) {
                        console.log('Proj Loc Data Loading...');
