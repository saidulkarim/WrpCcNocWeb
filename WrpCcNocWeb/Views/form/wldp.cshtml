                } else {
                    Notification('warning', 'Required Field', 'Please enter name!');
                    jQuery("#NameOfHydroSystem").focus();
                }
            } else {
                Notification('warning', 'Required Field', 'Please select a hydrological system!');
                jQuery("#HydroSystemCategoryId").focus();
            }
        });

