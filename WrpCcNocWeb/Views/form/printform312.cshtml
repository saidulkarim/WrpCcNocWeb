@using WrpCcNocWeb.DatabaseContext
@using WrpCcNocWeb.Helpers

@{
    Layout = "~/Views/Shared/_ReportLayout.cshtml";

    WrpCcNocDbContext _db = new WrpCcNocDbContext();
    CcModAppProjectCommonDetail pcd = (CcModAppProjectCommonDetail)ViewData["ProjectCommonDetail"];
    CcModAppProject_312_IndvDetail projectindi = (CcModAppProject_312_IndvDetail)ViewData["ProjectIndvDetail312"];

    var prev_lang = ViewData["LanguageId"].ToString();
    var app_state_id = pcd.ApplicationStateId;
    var app_state = (string)ViewData["ApplicationState"];
    var is_complete_id = pcd.IsCompletedId;
    var review_cycle_no = pcd.ReviewCycleNo;

    var location = ViewData["ProjectLocationDetail"];
    var hrd = ViewData["HydroRegionDetail"];
    var hsd = ViewData["BDP2100HotSpotDetail"];

    var pwu = ViewData["PurposeOfWaterUseDetail"];
    var tpw = ViewData["TypeProposedWellDetail"];
    var gwd = ViewData["GrndWtrDepthDetail"];
    var coa = ViewData["CategoryOfAquiferDetail"];
    var gwq = ViewData["GroundWaterQuality"];
    var pgw = ViewData["PotentialGroundWaterRechargeDetail"];
    var std = ViewData["SoilTypeDetail"];
    var wuc = ViewData["WaterUseConsumptionDetail"];
    var ica = ViewData["IrrigCropAreaDetailTemp"];

    var cnwpd = ViewData["CompatNWPDetail"];
    var cnwmpd = ViewData["CompatNWMPDetail"];
    var sdgd = ViewData["CompatSDGDetail"];
    var sdgid = ViewData["CompatSDGIndiDetail"];
    var bdp2100gd = ViewData["BDP2100GoalDetail"];
    var gpwmgtd = ViewData["GPWMGroupType"];

    List<DataAnalysisControlComments> fdac = (List<DataAnalysisControlComments>)ViewData["FormCommentsListTemp"];

    long ProjectId = (long)ViewData["ProjectId"];
    long ProjectIndvId = (long)ViewData["Project312IndvId"];
    long UserId = (long)ViewData["UserId"];
    int ProjectTypeId = (int)ViewData["ProjectTypeId"];
    var user_group_id = ViewData["UserLevel"].ToString();
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <input id="pProjectId" type="hidden" value="@ProjectId" />
                <input id="pProjectIndvId" type="hidden" value="@ProjectIndvId" />
                <input id="pUserId" type="hidden" value="@UserId" />
                <input id="pProjectTypeId" type="hidden" value="@ProjectTypeId" />

                <center>
                    <div class="col-md-12 text-center">
                        <h3 class="HeaderTitle">
                            @if (prev_lang == "1")
                            {<span>ছাড়পত্রের সনদের জন্য আবেদন</span>}
                            else
                            {<span>Application for Clearance Certificate</span>}
                        </h3>
                        <h4 class="HeaderSubTitle">
                            @if (prev_lang == "1")
                            {<span>(@ViewData["ProjectTypeTitleBn"].ToString())</span>}
                        else
                        {<span>(@ViewData["ProjectTypeTitle"].ToString())</span>}
                        </h4>
                    </div>
                </center>

                <div id="form-preview" class="form-preview container">
                    <div class="row lineGap">
                        <div class="col">
                            <span>
                                @if (prev_lang == "1")
                                {<span>বরাবর</span>}
                                else
                                {<span>To</span>}
                            </span><br />
                            <span>
                                @if (prev_lang == "1")
                                {<span>কার্যনির্বাহী কমিটি</span>}
                                else
                                {<span>Executive Committee</span>}
                            </span><br />
                            <span>
                                @if (prev_lang == "1")
                                {<span>ন্যাশনাল ওয়াটার রিসোর্সেস প্রজেক্ট কমিটি।</span>}
                                else
                                {<span>National Water Resources Project Committee.</span>}
                            </span>
                        </div>
                    </div>

                    <br />

                    <div class="row lineGap">
                        <div class="col">
                            <p>
                                @if (prev_lang == "1")
                                {<strong>মাধ্যম:</strong>}
                                else
                                {<strong>Through:</strong>}

                                @if (prev_lang == "1")
                                {<span>মহাপরিচালক, পানি সম্পদ পরিকল্পনা সংস্থা।</span>}
                                else
                                {<span>Director General, Water Resources Planning Organization.</span>}
                            </p>
                        </div>
                    </div>

                    <br />

                    <div class="row lineGap">
                        <div class="col">
                            <p>
                                @if (prev_lang == "1")
                                {<span>আমি, নীচে স্বাক্ষরিত, বন্যা নিয়ন্ত্রণ/ পরিচালনা সম্পর্কিত একটি প্রকল্প শুরু এবং বাস্তবায়নে ইচ্ছুক এবং আমি এখানে প্রাসঙ্গিক সংযুক্তিগুলির সাথে ছাড়পত্রের জন্য অনুরোধ করছি।</span>}
                                else
                                {<span>I, the undersigned, willing to initiate and implement a project on Flood Control/Management, And I am hereby requesting for clearance certificate with relevant attachments.</span>}
                            </p>
                        </div>
                    </div>

                    <br />

                    <div class="row lineGap">
                        <div class="col-md-12">
                            <h4>
                                <strong>
                                    @if (prev_lang == "1")
                                    {<span>আবেদনকারীর পুরো নাম এবং ঠিকানা:</span>}
                                    else
                                    {<span>Full Name and Address of Applicant:</span>}
                                </strong>
                            </h4>

                            @if (prev_lang == "1")
                            {
                                @ViewData["ApplicantNameBn"].ToString()<br />
                                @ViewData["ApplicantAddressBn"].ToString()<br />
                                <span>যোগাযোগ:</span> @ViewData["ApplicantMobileBn"].ToString()<br />
                                <span>ইমেইল:</span> @ViewData["ApplicantEmail"].ToString()
                            }
                            else
                            {
                                @ViewData["ApplicantName"].ToString()<br />
                                @ViewData["ApplicantAddress"].ToString()<br />
                                <span>Mobile:</span> @ViewData["ApplicantMobile"].ToString()<br />
                                <span>Email:</span> @ViewData["ApplicantEmail"].ToString()
                            }
                        </div>
                    </div>

                    <br />

                    <table style="border: 1px solid #808080; table-layout: fixed; width: 1500px;" border="1" cellpadding="1" cellspacing="0">
                        <tr style="background: #9d9b9b; font-weight: bold;">
                            <td colspan="3">
                                <h4>
                                    @if (prev_lang == "1")
                                    {<span>১. সাধারণ তথ্য</span>}
                                    else
                                    {<span>A. General Information</span>}
                                </h4>
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 200px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>i. প্রকল্পের নাম</span>}
                                    else
                                    {<span>i. Project Name</span>}
                                </b>
                            </td>
                            <td style="width: 900px; max-width: 700px; word-break: break-all;">@pcd.ProjectName</td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "ProjectName").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td>
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>ii. প্রকল্পের পূর্ব ইতিহাস ও যৌক্তিকতা</span>}
                                    else
                                    {<span>ii. Project Background and Rationale</span>}
                                </b>
                            </td>
                            <td style="word-break: break-all;">@pcd.BackgroundAndRationale</td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "BackgroundAndRationale").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>
                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>iii. প্রকল্পের লক্ষ্য</span>}
                                    else
                                    {<span>iii. Project Target</span>}
                                </b>
                            </td>
                            <td>@pcd.ProjectTarget</td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "ProjectTarget").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>iv. প্রকল্পের উদ্দেশ্য</span>}
                                    else
                                    {<span>iv. Project Objective</span>}
                                </b>
                            </td>
                            <td>@pcd.ProjectObjective</td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "ProjectObjective").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>v. প্রকল্পের ক্রিয়াকলাপ / উপাদানসমূহ</span>}
                                    else
                                    {<span>v. Project Activities/ Components</span>}
                                </b>
                            </td>
                            <td>@pcd.ProjectActivity</td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "ProjectActivity").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>vi. প্রকল্প ব্যয়</span>}
                                    else
                                    {<span>vi. Estimated Project Cost</span>}
                                </b>
                            </td>
                            <td>
                                @if (prev_lang == "1")
                                {<span>@pcd.ProjectEstimatedCost.ToString().NumberEnglishToBengali()</span>}
                            else
                            { <span>@pcd.ProjectEstimatedCost</span>}
                            </td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "ProjectEstimatedCost").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>
                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>vii. প্রকল্প সময়কাল</span>}
                                    else
                                    {<span>vii. Project Period</span>}
                                </b>
                            </td>
                            <td>
                                @if (prev_lang == "1")
                                {
                                    <span>প্রাম্ভিক কাল: @pcd.ProjectStartDate.Value.ToString("dd MMMM, yyyy").MonthEnglishToBengali().NumberEnglishToBengali()</span>
                                }
                                else
                                {
                                    <span>Starting Time: @pcd.ProjectStartDate.Value.ToString("dd MMMM, yyyy")</span>
                                }

                                <br />

                                @if (prev_lang == "1")
                                {
                                    <span>সমাপ্তি কাল: @pcd.ProjectCompletionDate.Value.ToString("dd MMMM, yyyy").MonthEnglishToBengali().NumberEnglishToBengali()</span>
                                }
                                else
                                {
                                    <span>Completion Time: @pcd.ProjectCompletionDate.Value.ToString("dd MMMM, yyyy")</span>
                                }
                            </td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "ProjectPeriod").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>
                        <tr style="vertical-align: top; text-align: left;">
                            <td colspan="3">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>viii. প্রকল্পের অবস্থান</span>}
                                    else
                                    {<span>viii. Project Location</span>}
                                </b>
                                &nbsp;&nbsp;
                                <a id="btnMapView" href="javascript:void(0);" target="_blank" class="btn btn-sm btn-info waves-effect"
                                   data-toggle="tooltip" data-placement="top" title="" data-original-title="View Map"
                                   style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                                    <i class="mdi mdi-map"></i>
                                </a>
                            </td>
                        </tr>
                        <tr style="vertical-align: top; text-align: left;">
                            <td colspan="3">
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "ProjectPeriod").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>
                        <tr>
                            <td colspan="3">
                                <table style="width:100%; border: 1px solid #808080; text-align: center;" border="1">
                                    <thead style="background: #9d9b9b; font-weight: bold;">
                                        <tr>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>ইউনিয়ন</span>}
                                                else
                                                {<span>Union</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>উপজেলা</span>}
                                                else
                                                {<span>Upazila</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>জেলা</span>}
                                                else
                                                {<span>District</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>অক্ষাংশ</span>}
                                                else
                                                {<span>Latitude</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>দ্রাঘিমাংশ</span>}
                                                else
                                                {<span>Longitude</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>সংযুক্তি</span>}
                                                else
                                                {<span>Attchments</span>}
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody id="tbodyProjectLocationPreview">
                                        @foreach (ProjectLocationTemp loc in location as List<ProjectLocationTemp>)
                                        {
                                            if (prev_lang == "1")
                                            {
                                                <tr>
                                                    <td>@loc.UnionNameBn</td>
                                                    <td>@loc.UpazilaNameBn</td>
                                                    <td>@loc.DistrictNameBn</td>
                                                    <td>@loc.LatitudeBn</td>
                                                    <td>@loc.LongitudeBn</td>
                                                    <td style="word-break: break-all;"><a href="@loc.ImageFileName" target="_blank">@loc.OnlyImageFileName</a></td>
                                                </tr>
                                            }
                                            else
                                            {
                                                <tr>
                                                    <td>@loc.UnionName</td>
                                                    <td>@loc.UpazilaName</td>
                                                    <td>@loc.DistrictName</td>
                                                    <td>@loc.Latitude</td>
                                                    <td>@loc.Longitude</td>
                                                    <td style="word-break: break-all;"><a href="@loc.ImageFileName" target="_blank">@loc.OnlyImageFileName</a></td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                            </td>
                        </tr>

                        <tr style="border-left: none !important; border-right: none !important;">
                            <td colspan="3">
                            </td>
                        </tr>

                        <tr style="background: #9d9b9b; font-weight: bold;">
                            <td colspan="3">
                                <h4>
                                    @if (prev_lang == "1")
                                    {<span>২. কারিগরি তথ্য</span>}
                                    else
                                    {<span>B. Technical Information</span>}
                                </h4>
                            </td>
                        </tr>

                        <tr>
                            <td colspan="3">
                                <h5>
                                    @if (prev_lang == "1")
                                    {<span>২.১ বেজলাইন সম্পর্কিত তথ্য বা অবস্থা পর্যালোচনা</span>}
                                    else
                                    {<span>2.1 Discussion about baseline related information or situation</span>}
                                </h5>
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>i. নলকূপের বিবরণ</span> }
                                    else
                                    { <span>i. Well Description</span>}
                                </b>
                            </td>
                            <td>@projectindi.WellDescription</td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "WellDescription").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>ii. পানি ব্যবহারের উদ্দেশ্য</span> }
                                    else
                                    { <span>ii. Purpose of Water Use</span>}
                                </b>
                            </td>
                            <td>
                                <ol>
                                    @foreach (var _pwu in (List<PurposeOfWaterUseDetailTemp>)pwu)
                                    {
                                        if (prev_lang == "1")
                                        {
                                            <li>@_pwu.WaterUsePurposeBn</li>
                                        }
                                        else
                                        {
                                            <li>@_pwu.WaterUsePurpose</li>
                                        }
                                    }
                                </ol>
                            </td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "PurposeWaterUse").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>iii. হাইড্রোলজিক্যাল অঞ্চল</span>}
                                    else
                                    {<span>iii. Hydrological Region</span>}
                                </b>
                            </td>
                            <td>
                                <table style="border: 1px solid #808080; width: 100%; text-align: center;" border="1">
                                    <tbody>@Html.Raw(hrd.ToString())</tbody>
                                </table>
                            </td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "HydrologicalRegion").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>iv. বাংলাদেশ ব-দ্বীপ পরিকল্পনা-২১০০ হট স্পট</span>}
                                    else
                                    {<span>iv. Bangladesh Delta Plan 2100 Hot Spot</span>}
                                </b>
                            </td>
                            <td>
                                <ol>
                                    @foreach (var bdphsd in (List<BDP2100HotSpotDetailTemp>)ViewData["BDP2100HotSpotDetail"])
                                    {
                                        if (prev_lang == "1")
                                        {
                                            <li>@bdphsd.PlanNameBn</li>
                                        }
                                        else
                                        {
                                            <li>@bdphsd.PlanName</li>
                                        }
                                    }
                                </ol>
                            </td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "BDP2100HotSpot").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>v. বার্ষিক বৃষ্টিপাত (বিগত পাঁচ বছর) (মি.মি.)</span>}
                                    else
                                    {<span>v. Annual Rainfall (last five years)</span>}
                                </b>
                            </td>
                            <td>
                                <table style="width:100%; border: 1px solid #808080; text-align: center;" border="1">
                                    <tr>
                                        <th class="text-center">
                                            @{
                                                int year1 = DateTime.Now.Year - 1;
                                                <span>@year1</span>
                                            }
                                        </th>
                                        <th class="text-center">
                                            @{
                                                int year2 = DateTime.Now.Year - 2;
                                                <span>@year2</span>
                                            }
                                        </th>
                                        <th class="text-center">
                                            @{
                                                int year3 = DateTime.Now.Year - 3;
                                                <span>@year3</span>
                                            }
                                        </th>
                                        <th class="text-center">
                                            @{
                                                int year4 = DateTime.Now.Year - 4;
                                                <span>@year4</span>
                                            }
                                        </th>
                                        <th class="text-center">
                                            @{
                                                int year5 = DateTime.Now.Year - 5;
                                                <span>@year5</span>
                                            }
                                        </th>
                                    </tr>
                                    <tr>
                                        <td id="pAnnualRainFallLast1Year">@pcd.AnnualRainFallLast1Year</td>
                                        <td id="pAnnualRainFallLast2Years">@pcd.AnnualRainFallLast2Years</td>
                                        <td id="pAnnualRainFallLast3Years">@pcd.AnnualRainFallLast3Years</td>
                                        <td id="pAnnualRainFallLast4Years">@pcd.AnnualRainFallLast4Years</td>
                                        <td id="pAnnualRainFallLast5Years">@pcd.AnnualRainFallLast5Years</td>
                                    </tr>
                                </table>
                            </td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "AnnualRainfallLastFiveYears").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>vi. প্রস্তাবিত কূপের ধরণ</span> }
                                    else
                                    { <span>vi. Type of Proposed Well</span>}
                                </b>
                            </td>
                            <td>
                                @foreach (TypeProposedWellDetailTemp _tpw in (List<TypeProposedWellDetailTemp>)tpw)
                                {
                                    if (prev_lang == "1")
                                    {
                                        @_tpw.ProposedWellNameBn
                                    }
                                    else
                                    {
                                        @_tpw.ProposedWellName
                                    }
                                }
                            </td>
                            <td>
                                @Html.Raw(fdac.Where(w => w.ControlTitle == "TypeProposedWell").Select(s => s.Comments).FirstOrDefault())
                            </td>
                        </tr>

