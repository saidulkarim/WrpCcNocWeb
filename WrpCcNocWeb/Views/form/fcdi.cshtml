                                                                                                    <td class="text-center">
                                                                                                        <input type="number" min="0" id="BankErosionRate" name="BankErosionRate" class="form-control" />
                                                                                                    </td>
                                                                                                </tr>
                                                                                            </tbody>
                                                                                        </table>
                                                                                    </div>
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group">
                                                                                <label for="BankStability">
                                                                                    @{if (language == 1)
                                                                                        {<span>x. পাড় স্থিতিশীলতা</span>}
                                                                                        else
                                                                                        {<span>x. Bank Stability</span>}
                                                                                    }
                                                                                </label>
                                                                                <div id="BankStability" class="row" style="margin-left: 5px;">
                                                                                    @{
                                                                                        List<LookUpCcModBankStability> _bnst = ViewBag.LookUpCcModBankStability;

                                                                                        if (_bnst.Count > 0)
                                                                                        {
                                                                                            foreach (LookUpCcModBankStability bs in _bnst)
                                                                                            {
                                                                                                var id = "radBankStability" + bs.BankStabilityTypeId;

                                                                                                if (language == 1)
                                                                                                {
                                                                                                    <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                        <input type="radio" name="BankStabilityTypeId" id="@id" value="@bs.BankStabilityTypeId" data-parsley-multiple="BankStabilityTypeId">
                                                                                                        <label for="@id"> @bs.BankStabilityTypeBn </label>
                                                                                                    </div>
                                                                                                }
                                                                                                else
                                                                                                {
                                                                                                    <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                        <input type="radio" name="BankStabilityTypeId" id="@id" value="@bs.BankStabilityTypeId" data-parsley-multiple="BankStabilityTypeId">
