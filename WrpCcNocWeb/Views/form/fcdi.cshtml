            } else {
                title = "Are you sure?";
                text = "Please make sure the data you have provided that has been saved successfully!";
            }

            Swal.fire({
                title: title,
                text: text,
                type: "warning",
                allowEscapeKey: false,
                allowOutsideClick: false,
                showCancelButton: !0,
                confirmButtonColor: "#3085d6",
