@using static WrpCcNocWeb.ViewModels.GeneralInfo
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var language = HttpContextAccessor.HttpContext.Request.Cookies["FormLanguage"].ToString();
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <h4 class="header-title mb-3">
                    <b>@ViewBag.ProjectTitle</b>
                    <button id="btnGoBack" type="button" class="btn btn-secondary float-right" style="margin-top: -10px;">Back</button>
                </h4>

                <div id="basicwizard">
                    <ul class="nav nav-pills bg-light nav-justified form-wizard-header">
                        <li class="nav-item">
                            <a id="lnkTab1" href="#basictab1" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2 active">
                                <i class="mdi mdi-format-float-right mr-1"></i>
                                <span class="d-none d-sm-inline">
                                    @if (language == "1")
                                    {
                                        <span>আবেদন পত্র</span>
                                    }
                                    else
                                    {
                                        <span>Application Form</span>
                                    }
                                </span>
                            </a>
                        </li>
                        @*<li class="nav-item">
                                <a id="lnkTab2" href="#basictab2" data-toggle="tab" class="nav-link rounded-0 pt-2 pb-2">
                                    <i class="mdi mdi-face-profile mr-1"></i>
                                    <span class="d-none d-sm-inline">
                                        @if (language == "1")
                                        {
                                            <span>প্রিভিউ</span>
                                        }
                                        else
                                        {
                                            <span>Preview</span>
                                        }
                                    </span>
                                </a>
                            </li>*@
                    </ul>

                    <div class="tab-content b-0 mb-0">
                        <div class="tab-pane active" id="basictab1">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="card-box">
                                        <div class="row">
                                            @Html.Hidden("ProjectId")
                                            @Html.Hidden("AppSubmissionId")
                                            @Html.Hidden("ApplicationStateId")
                                            @Html.Hidden("ApprovalStatusId")
                                            @Html.Hidden("Project33IndvId")
                                            @Html.Hidden("UserId")

                                            <input id="ProjectTypeId" type="hidden" value="@ViewBag.ProjectTypeId" />

                                            <div class="col-sm-3">
												@{ Html.RenderPartial("form/_1TabPanel"); }
                                            </div> <!-- end col-->
                                            <div class="col-sm-9">
                                                <div class="tab-content pt-0">
                                                    <div class="tab-pane fade active show" id="v-gen-info" role="tabpanel" aria-labelledby="v-gen-info-tab">
                                                        <form id="frmGeneralInfo">
                                                            <div class="card-box">
																@{ Html.RenderPartial("form/_2GeneralInfo"); }
                                                            </div>
                                                        </form>
                                                    </div>

                                                    <div class="tab-pane fade" id="v-project-location" role="tabpanel" aria-labelledby="v-project-location-tab">
                                                        <div class="card-box">
															@{ Html.RenderPartial("form/_3Location"); }
                                                        </div>
                                                    </div>

                                                    <div class="tab-pane fade" id="v-tech-info" role="tabpanel" aria-labelledby="v-tech-info-tab">
                                                        <form id="frmTechnicalInfo">
                                                            <div class="card-box">
                                                                <div id="tech-info-form">
                                                                    <div class="card">
                                                                        <div class="card-header bg-blue py-3 text-white cursor" data-toggle="collapse" href="#cardCollpase31">
                                                                            <div class="card-widgets">
                                                                                <a data-toggle="collapse" href="#cardCollpase31" role="button" aria-expanded="false" aria-controls="cardCollpase2"><i class="mdi mdi-minus"></i></a>
                                                                            </div>
                                                                            <h5 class="card-title mb-0 text-white">
                                                                                @if (language == "1")
                                                                                {
                                                                                    <span>১. বেজলাইন সম্পর্কিত তথ্য বা অবস্থা পর্যালোচনা</span>}
                                                                                else
                                                                                {
                                                                                    <span>1. Discussion about baseline related information or situation</span>}
                                                                            </h5>
                                                                        </div>
                                                                        <div id="cardCollpase31" class="collapse show">
                                                                            <div class="card-body">
                                                                                <div class="form-group">
                                                                                    <label for="HydrologicalRegion">
                                                                                        @if (language == "1")
                                                                                        {<span>হাইড্রোলজিক্যাল অঞ্চল</span>}
                                                                                        else
                                                                                        {<span> Hydrological Region</span>}
                                                                                    </label>
                                                                                    <div id="chkHydrologicalRegions" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModHydroRegion> _hydro_regions = ViewBag.LookUpCcModHydroRegion;

                                                                                            if (_hydro_regions.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModHydroRegion hr in _hydro_regions)
                                                                                                {
                                                                                                    var id = "chkHydroRegion" + hr.HydroRegionId;

                                                                                                    if (language == "1")
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullNameBn">
                                                                                                            <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                            <label for="@id"> @hr.HydroRegionFullNameBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullName">
                                                                                                            <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                                                                            <label for="@id"> @hr.HydroRegionFullName </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="BDP2100HotSpot">
                                                                                        @{if (language == "1")
                                                                                            { <span>বাংলাদেশ ব-দ্বীপ পরিকল্পনা-২১০০  হট স্পট</span>}
                                                                                            else
                                                                                            {<span>  Bangladesh Delta Plan 2100 Hot Spot </span>}
                                                                                        }
                                                                                    </label>
                                                                                    <div id="chkBDP2100HotSpot" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModDeltPlan2100HotSpot> _hot_spots = ViewBag.LookUpCcModDeltPlan2100HotSpot;

                                                                                            if (_hot_spots.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModDeltPlan2100HotSpot hs in _hot_spots)
                                                                                                {
                                                                                                    var id = "chkBDP2100HotSpot" + hs.DeltaPlanHotSpotId;

                                                                                                    if (language == "1")
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanNameBn">
                                                                                                            <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                            <label for="@id"> @hs.PlanNameBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanName">
                                                                                                            <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                                                                            <label for="@id"> @hs.PlanName </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="NameOfTheWaterBody">
                                                                                        @{if (language == "1")
                                                                                            { <span>পানি উত্তোলনের উৎস (নদী/ খাল/ অন্যান্য)</span>}
                                                                                            else
                                                                                            {<span>Name of the Water Bodies to be Used (River/ Khal/ Others)</span>}
                                                                                        }
                                                                                    </label>
                                                                                    <input type="text" id="NameOfTheWaterBody" name="NameOfTheWaterBody" class="form-control" />
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="WaterBody">
                                                                                        @{if (language == "1")
                                                                                            {<span>প্রবাহের ধরন</span>}
                                                                                            else
                                                                                            {<span>Type of Water Bodies</span>}
                                                                                        }
                                                                                    </label>
                                                                                    <div id="WaterBody" class="row" style="margin-left: 5px;">
                                                                                        @{
                                                                                            List<LookUpCcModTypeOfWaterBody> _towb = ViewBag.LookUpCcModTypeOfWaterBody;

                                                                                            if (_towb.Count > 0)
                                                                                            {
                                                                                                foreach (LookUpCcModTypeOfWaterBody wb in _towb)
                                                                                                {
                                                                                                    var id = "radWaterBody" + wb.WaterBodyTypeId;

                                                                                                    if (language == "1")
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyTypeId" id="@id" value="@wb.WaterBodyTypeId" data-parsley-multiple="WaterBodyTypeId">
                                                                                                            <label for="@id"> @wb.WaterBodyTypeBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                                                                            <input type="radio" name="WaterBodyTypeId" id="@id" value="@wb.WaterBodyTypeId" data-parsley-multiple="WaterBodyTypeId">
                                                                                                            <label for="@id"> @wb.WaterBodyType </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div id="divOfftakeOutfallConnectivity">
                                                                                    <label for="OfftakeOutfallConnectivity" class="bold">
                                                                                        @{if (language == "1")
                                                                                            { <span>উৎস মুখ, পতিত মুখ এবং সংযোগ</span>}
                                                                                            else
                                                                                            {<span>Offtake, Outfall and Connectivity</span>}
                                                                                        }
                                                                                    </label>

                                                                                    <hr style="margin-top: 0 !important;" />

                                                                                    <div class="row">
                                                                                        <div class="form-group col-xl-4 text-center">
                                                                                            <label for="Offtake">
                                                                                                @{if (language == "1")
                                                                                                    { <span>উৎস মুখ</span>}
                                                                                                    else
                                                                                                    {<span>Offtake</span>}
                                                                                                }
                                                                                            </label>
                                                                                            <input type="number" id="Offtake" name="Offtake" class="form-control" />
                                                                                        </div>

                                                                                        <div class="form-group col-md-4 text-center">
                                                                                            <label for="Outfall">
                                                                                                @{if (language == "1")
                                                                                                    { <span>পতিত মুখ</span>}
                                                                                                    else
                                                                                                    {<span>Outfall</span>}
                                                                                                }
                                                                                            </label>
                                                                                            <input type="number" id="Outfall" name="Outfall" class="form-control" />
                                                                                        </div>

                                                                                        <div class="form-group col-xl-4 text-center">
                                                                                            <label for="Connectivity">
                                                                                                @{if (language == "1")
                                                                                                    { <span>অন্যান্য নদ-নদী/ খাল বিলের সাথে সংযোগ</span>}
                                                                                                    else
                                                                                                    {<span>Connectivity with Other Waterbodies</span>}
                                                                                                }
                                                                                            </label>
                                                                                            <input type="number" id="Connectivity" name="Connectivity" class="form-control" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="ExistingUseOfWaterFromSource">
                                                                                        @{if (language == "1")
                                                                                            { <span>উৎস হতে বিদ্যমান পানির ব্যবহার</span>}
                                                                                            else
                                                                                            {<span>Existing Use of Water from Source</span>}
                                                                                        }
                                                                                    </label>
                                                                                    <input type="text" id="ExistingUseOfWaterFromSource" name="ExistingUseOfWaterFromSource" class="form-control" />
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="HydrologicalSystem">
                                                                                        @{if (language == "1")
                                                                                            { <span>ইনটেক পয়েন্ট হতে নিকটস্থ নদী/ খাল/ বিলের দূরত্ব</span>}
                                                                                            else
                                                                                            {<span>Surrounding waterbodies with distance from intake point</span>}
                                                                                        }
                                                                                    </label>

                                                                                    <div id="HydrologicalSystem" class="row" style="margin-left: -7px;">
                                                                                        <div class="table-responsive">
                                                                                            <table class="table mb-0">
                                                                                                <thead>
                                                                                                    <tr>
                                                                                                        <th class="text-center">#</th>
                                                                                                        <th class="text-center">
                                                                                                            @{if (language == "1")
                                                                                                                {
                                                                                                                    <span>নদী/ খাল/ বিল</span>}
                                                                                                                else
                                                                                                                {
                                                                                                                    <span>Water Bodies (River/ Khal/ Beel/ Others)</span>}
                                                                                                            }
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            <span id="HydroSystemName"></span>
                                                                                                            @{if (language == "1")
                                                                                                                {
                                                                                                                    <span>নাম</span>}
                                                                                                                else
                                                                                                                {
                                                                                                                    <span>Name</span>}
                                                                                                            }
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            @{if (language == "1")
                                                                                                                {
                                                                                                                    <span>দৈর্ঘ্য বা অঞ্চল</span>}
                                                                                                                else
                                                                                                                {
                                                                                                                    <span>Length or Area</span>}
                                                                                                            }
                                                                                                        </th>
                                                                                                        <th class="text-center"></th>
                                                                                                    </tr>
                                                                                                    <tr>
                                                                                                        <th colspan="2" class="text-center">
                                                                                                            <input id="HydroSysDetailId" type="hidden" />
                                                                                                            <select id="HydroSystemCategoryId" name="HydroSystemCategoryId" class="form-control">
                                                                                                                <option value="">
                                                                                                                    @{if (language == "1")
                                                                                                                        { <span>বাছাই করুন...</span>}
                                                                                                                        else
                                                                                                                        {<span>Choose...</span>}
                                                                                                                    }
                                                                                                                </option>
                                                                                                                @{
                                                                                                                    List<LookUpCcModHydroSystem> _hs = ViewBag.LookUpCcModHydroSystem;

                                                                                                                    if (_hs.Count > 0)
                                                                                                                    {
                                                                                                                        foreach (LookUpCcModHydroSystem hs in _hs)
                                                                                                                        {
                                                                                                                            if (language == "1")
                                                                                                                            {
                                                                                                                                <option value="@hs.HydroSystemCategoryId" title="@hs.HydroSysCategoryUnitBn">@hs.HydroSystemCategoryBn</option>
                                                                                                                            }
                                                                                                                            else
                                                                                                                            {
                                                                                                                                <option value="@hs.HydroSystemCategoryId" title="@hs.HydroSysCategoryUnit">@hs.HydroSystemCategory</option>
                                                                                                                            }
                                                                                                                        }
                                                                                                                    }
                                                                                                                }
                                                                                                            </select>
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            <input type="text" id="NameOfHydroSystem" name="NameOfHydroSystem" class="form-control" />
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            <div class="input-group">
                                                                                                                <input type="number" id="HydroSystemLengthArea" name="HydroSystemLengthArea" class="form-control" />
                                                                                                                <div class="input-group-append d-none">
                                                                                                                    <span id="HydroSysCategoryUnit" class="input-group-text">Unit</span>
                                                                                                                </div>
                                                                                                            </div>
                                                                                                        </th>
                                                                                                        <th class="text-center">
                                                                                                            <button id="btnSaveHydroSystem" type="button" class="btn btn-success waves-effect waves-light" value="Save">
                                                                                                                <i class="mdi mdi-content-save"></i>
                                                                                                            </button>

                                                                                                            <button id="btnClearHydroSystem" type="button" class="btn btn-warning waves-effect waves-light" value="Clear">
                                                                                                                <i class="mdi mdi-refresh"></i>
                                                                                                            </button>
                                                                                                        </th>
                                                                                                    </tr>
                                                                                                </thead>
                                                                                                <tbody id="tbodyHydrologicalSystem"></tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div id="divAnnualRainfall">
                                                                                    <label for="AnnualRainfall" class="bold">
                                                                                        @{if (language == "1")
                                                                                            { <span>বার্ষিক বৃষ্টিপাত (বিগত পাঁচ বছর) (মি.মি.)</span>}
                                                                                            else
                                                                                            {<span>Annual Rainfall (last five years)</span>}
                                                                                        }
                                                                                    </label>

                                                                                    <hr style="margin-top: 0 !important;" />

                                                                                    <div class="row">
                                                                                        <div class="form-group col-xl-3 text-center">
                                                                                            @{
                                                                                                int year1 = DateTime.Now.Year - 1;

                                                                                                <label for="AnnualRainFallLast1Year">@year1</label>
                                                                                                <input type="number" id="AnnualRainFallLast1Year" name="AnnualRainFallLast1Year" class="form-control" />
                                                                                            }
                                                                                        </div>

                                                                                        <div class="form-group col-md-2 text-center">
                                                                                            @{
                                                                                                int year2 = DateTime.Now.Year - 2;
                                                                                                <label for="AnnualRainFallLast2Years">@year2</label>
                                                                                                <input type="number" id="AnnualRainFallLast2Years" name="AnnualRainFallLast2Years" class="form-control" />
                                                                                            }
                                                                                        </div>

                                                                                        <div class="form-group col-xl-2 text-center">
                                                                                            @{
                                                                                                int year3 = DateTime.Now.Year - 3;
                                                                                                <label for="AnnualRainFallLast3Years">@year3</label>
                                                                                                <input type="number" id="AnnualRainFallLast3Years" name="AnnualRainFallLast3Years" class="form-control" />
                                                                                            }
                                                                                        </div>

                                                                                        <div class="form-group col-md-2 text-center">
                                                                                            @{
                                                                                                int year4 = DateTime.Now.Year - 4;
                                                                                                <label for="AnnualRainFallLast4Years">@year4</label>
                                                                                                <input type="number" id="AnnualRainFallLast4Years" name="AnnualRainFallLast4Years" class="form-control" />
                                                                                            }
                                                                                        </div>

                                                                                        <div class="form-group col-xl-3 text-center">
                                                                                            @{
                                                                                                int year5 = DateTime.Now.Year - 5;
                                                                                                <label for="AnnualRainFallLast5Years">@year5</label>
                                                                                                <input type="number" id="AnnualRainFallLast5Years" name="AnnualRainFallLast5Years" class="form-control" />
                                                                                            }
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="WaterLevelIntakePoint">
                                                                                        @{if (language == "1")
                                                                                            { <span>ইনটেক পয়েন্টে পানির সমতল (ওয়াটার লেভেল)</span>}
                                                                                            else
                                                                                            {<span>Water Level (m PWD) at Intake Point</span>}
                                                                                        }
                                                                                    </label>

                                                                                    <div id="WaterLevelIntakePoint" class="row" style="margin-left: -7px;">
                                                                                        <div class="table-responsive">
                                                                                            <table class="table mb-0">
                                                                                                <thead>
                                                                                                    <tr>
                                                                                                        <th class="text-center" colspan="2">Dry Season</th>
                                                                                                        <th class="text-center" colspan="2">Wet Season</th>
                                                                                                    </tr>
                                                                                                    <tr>
                                                                                                        <th class="text-center">Maximum</th>
                                                                                                        <th class="text-center">Minimum</th>
                                                                                                        <th class="text-center">Maximum</th>
                                                                                                        <th class="text-center">Minimum</th>
                                                                                                    </tr>
                                                                                                </thead>
                                                                                                <tbody>
                                                                                                    <tr>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" id="WaterLevelDryMax" name="WaterLevelDryMax" class="form-control" />
                                                                                                        </td>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" id="WaterLevelDryMin" name="WaterLevelDryMin" class="form-control" />
                                                                                                        </td>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" id="WaterLevelWetMax" name="WaterLevelWetMax" class="form-control" />
                                                                                                        </td>
                                                                                                        <td class="text-center">
                                                                                                            <input type="number" id="WaterLevelWetMin" name="WaterLevelWetMin" class="form-control" />
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                </tbody>
                                                                                            </table>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="DischargeIntakePoint">
                                                                                        @{if (language == "1")
                                                                                            { <span>ইনটেক পয়েন্টে পানির প্রবাহ (ডিসচার্জ); (কিউমেক)</span>}
                                                                                            else
                                                                                            {<span>Discharge (m<sup>3</sup>/s) at Intake Point</span>}
                                                                                        }
                                                                                    </label>

                                                                                    <div id="DischargeIntakePoint" class="row" style="margin-left: -7px;">
                                                                                        <div class="table-responsive">
                                                                                            <table class="table mb-0">
                                                                                                <thead>
                                                                                                    <tr>
                                                                                                        <th class="text-center" colspan="2">Dry Season</th>
                                                                                                        <th class="text-center" colspan="2">Wet Season</th>
                                                                                                    </tr>
                                                                                                    <tr>
                                                                                                        <th class="text-center">Maximum</th>
                                                                                                        <th class="text-center">Minimum</th>
                                                                                                        <th class="text-center">Maximum</th>
                                                                                                        <th class="text-center">Minimum</th>
                                                                                                    </tr>
                                                                                                </thead>
                                                                                                <tbody>
                                                                                                    <tr>
                                                                                                        <td class="text-center">
