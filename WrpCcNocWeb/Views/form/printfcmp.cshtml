                                <b>
                                    @if (prev_lang == "1")
                                    {<span>viii. সর্বোচ্চ  পানির প্রবাহ (ডিসচার্জ) (কিউসেক)</span>}
                                    else
                                    {<span>viii. Maximum Discharge (m<sup>3</sup>/s)</span>}
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">@project31indev.MaximumDischarge</td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>ix. বন্যার ধরণ</span>}
                                    else
                                    {<span>ix. Types of Flood</span>}
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">
                                <ol>
                                    @{
                                        foreach (TypesOfFloodTemp tof in (List<TypesOfFloodTemp>)tofd)
                                        {
                                            if (prev_lang == "1")
                                            {
                                                <li>@tof.FloodTypeNameBn</li>
                                            }
                                            else
                                            {
                                                <li>@tof.FloodTypeName</li>
                                            }
                                        }
                                    }
                                </ol>
                            </td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>x. ফ্লাড ফ্রিকোয়েন্সি এনালাইসিস (মিটার, পিডব্লিউডি)</span>}
                                    else
                                    {<span>x. Flood Frequency Analysis (Maximum water level in m, PWD)</span>}
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">
                                <table style="width:100%; border: 1px solid #808080; text-align: center;" border="1">
                                    <thead>
                                        <tr>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>ফ্লাড ফ্রিকোয়েন্সি</span>}
                                                else
                                                {<span>Flood Frequency</span>}
                                            </th>
                                            <th class="text-center">
                                                @if (prev_lang == "1")
                                                {<span>ফ্রিকোয়েন্সি</span>}
                                                else
                                                {<span>Frequency</span>}
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody id="tbodyFloodFrequencyPreview">
                                        @{
                                            foreach (FloodFrequencyDetailTemp ffdt in (List<FloodFrequencyDetailTemp>)ffd)
                                            {
                                                if (prev_lang == "1")
                                                {
                                                    <tr>
                                                        <td>@ffdt.FloodFrequencyBn</td>
                                                        <td>@ffdt.FloodFrequencyLevel.NumberEnglishToBengali()</td>
                                                    </tr>
                                                }
                                                else
                                                {
                                                    <tr>
                                                        <td>@ffdt.FloodFrequency</td>
                                                        <td>@ffdt.FloodFrequencyLevel</td>
                                                    </tr>
                                                }
                                            }
                                        }
                                    </tbody>
                                </table>
                            </td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>xi. নিষ্কাশন (ড্রেইনেজ) ব্যবস্থা</span>}
                                    else
                                    {<span>xi. Drainage Condition</span>}
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">
                                @{
                                    var eng = project31indev.DrainageConditionId == 1 ? "Good" : project31indev.DrainageConditionId == 2 ? "Moderate" : project31indev.DrainageConditionId == 3 ? "Poor" : "Others";
                                    var ban = project31indev.DrainageConditionId == 1 ? "ভাল" : project31indev.DrainageConditionId == 2 ? "মধ্যপন্থী" : project31indev.DrainageConditionId == 3 ? "দরিদ্র" : "অন্যান্য";

                                    if (prev_lang == "1")
                                    {
                                        <span>@ban</span>
                                    }
                                    else
                                    {
                                        <span>@eng</span>
                                    }
                                }
                            </td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>xii. ভূপরিস্থ পানির গুনগত মান</span>}
                                    else
                                    {<span>xii. Surface Water Quality</span>}
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">
                                <table style="width:100%; border: 1px solid #808080; text-align: center;" border="1">
                                    <tr>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>লবনাক্ততা (পিপিএম)</span>}
                                            else
                                            {<span>Salinity (PPM)</span>}
                                        </th>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>ডিও (মি.গ্রা/লিটার)</span>}
                                            else
                                            {<span>DO (mg/l)</span>}
                                        </th>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>টি.ডি.এস</span>}
                                            else
                                            {<span>TDS</span>}
                                        </th>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>পিএইচ</span>}
                                            else
                                            {<span>pH</span>}
                                        </th>
                                    </tr>
                                    <tr>
                                        <td id="pWaterSalinity">@project31indev.WaterSalinity</td>
                                        <td id="pWaterDO">@project31indev.WaterDO</td>
                                        <td id="pWaterTDS">@project31indev.WaterTDS</td>
                                        <td id="pWaterPhLevel">@project31indev.WaterPhLevel</td>
                                    </tr>
                                </table>
                            </td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>xiii. ভূমির ধরন (শতকরা)</span>}
                                    else
                                    {<span>xiii. Land Type (Percent)</span>}
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">
                                <table style="width:100%; border: 1px solid #808080; text-align: center;" border="1">
                                    <tr>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>উচুঁ ভূমি <br />F<sub>0</sub> (০-৩০ সে.মি.)</span>}
                                            else
                                            {<span>High Land <br />F<sub>0</sub> (0-30 cm)</span>}
                                        </th>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>মধ্যম উচুঁ ভূমি <br />F<sub>1</sub> (৩০-৯০ সে.মি.)</span>}
                                            else
                                            {<span>Medium High Land <br />F<sub>1</sub> (30-90 cm)</span>}
                                        </th>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>মধ্যম নিচু ভূমি <br />F<sub>2</sub> (৯০-১৮০ সে.মি)</span>}
                                            else
                                            {<span>Medium Low Land <br />F<sub>2</sub> (90-180 cm)</span>}
                                        </th>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>নিচু ভূমি <br />F<sub>3</sub> (>১৮০-৩৬০ সে.মি.)</span>}
                                            else
                                            {<span>Low Land <br />F<sub>3</sub> (>180-360 cm</span>})
                                        </th>
                                        <th class="text-center">
                                            @if (prev_lang == "1")
                                            {<span>অত্যন্ত নিচু জমি <br />F<sub>4</sub> (>৩৬০ সে.মি)</span>}
                                            else
                                            {<span>Very Low Land <br />F<sub>4</sub> (>360 cm)</span>}
                                        </th>
                                    </tr>
                                    <tr>
                                        <td class="text-center">@project31indev.HighLandPercent</td>
                                        <td class="text-center">@project31indev.MediumHighLandPercent</td>
                                        <td class="text-center">@project31indev.MediumLowLandPercent</td>
                                        <td class="text-center">@project31indev.LowLandPercent</td>
                                        <td class="text-center">@project31indev.VeryLowLandPercent</td>
                                    </tr>
                                </table>
                            </td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    control_title
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">control_data</td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    control_title
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">control_data</td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>

                        <tr style="vertical-align: top; text-align: left;">
                            <td style="width: 250px;">
                                <b>
                                    control_title
                                </b>
                            </td>
                            <td style="width: 500px; word-wrap: break-word;">control_data</td>
                            <td style="word-wrap: break-word;">
                                control_comments
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>