                                                                                                            <input type="checkbox" name="ConservLocationId" id="@id" value="@cl.ConservLocationId" data-parsley-multiple="ConservLocationId">
                                                                                                            <label for="@id"> @cl.ConservLocation </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="WaterBody">
