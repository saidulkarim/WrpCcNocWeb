                                                                                                    if (language == 1)
                                                                                                    {
                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@stc.StructureNameBn">
                                                                                                            <input type="checkbox" name="TypeOfStructureId" id="@id" value="@stc.TypeOfStructureId" data-parsley-multiple="TypeOfStructureId">
                                                                                                            <label for="@id"> @stc.StructureNameBn </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                    else
