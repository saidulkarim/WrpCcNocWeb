                                                                                                        <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@stc.StructureName">
                                                                                                            <input type="checkbox" name="TypeOfStructureId" id="@id" value="@stc.TypeOfStructureId" data-parsley-multiple="TypeOfStructureId">
                                                                                                            <label for="@id"> @stc.StructureName </label>
                                                                                                        </div>
                                                                                                    }
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </div>
                                                                                </div>

                                                                                <div class="form-group">
                                                                                    <label for="ConservationLocation">
