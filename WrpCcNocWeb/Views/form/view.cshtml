@{
    ViewData["Title"] = "Project View";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var prev_lang = ViewBag.LanguageId.ToString();
    var app_state_id = ViewBag.ProjectCommonDetail.ApplicationStateId;
    var app_state = ViewBag.ApplicationState;
    var is_complete_id = ViewBag.ProjectCommonDetail.IsCompletedId;
    var review_cycle_no = ViewBag.ProjectCommonDetail.ReviewCycleNo;
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <input id="pProjectId" type="hidden" />
                <input id="pProject31IndvId" type="hidden" />
                <input id="pUserId" type="hidden" value="@ViewBag.UserId" />
                <input id="pProjectTypeId" type="hidden" />

                <div class="col-md-12 text-center">
                    <h3 class="HeaderTitle">
                        @if (prev_lang == "1")
                        {<span>ছাড়পত্রের সনদের জন্য আবেদন</span>}
                        else
                        {<span>Application for Clearance Certificate</span>}
                    </h3>
                    <h4 class="HeaderSubTitle">
                        (
                        @if (prev_lang == "1")
                        {<span>@ViewBag.ProjectTypeTitleBn</span>}
                    else
                    {<span>@ViewBag.ProjectTypeTitle</span>}
                        )
                    </h4>
                </div>

                <div id="form-preview" class="form-preview container">
                    <div class="row lineGap">
                        <div class="col">
                            <span>
                                @if (prev_lang == "1")
                                {<span>বরাবর</span>}
                                else
                                {<span>To</span>}
                            </span><br />
                            <span>
                                @if (prev_lang == "1")
                                {<span>কার্যনির্বাহী কমিটি</span>}
                                else
                                {<span>Executive Committee</span>}
                            </span><br />
                            <span>
                                @if (prev_lang == "1")
                                {<span>ন্যাশনাল ওয়াটার রিসোর্সেস প্রজেক্ট কমিটি।</span>}
                                else
                                {<span>National Water Resources Project Committee.</span>}
                            </span>
                        </div>
                    </div>
                    <br />
                    <div class="row lineGap">
                        <div class="col">
                            <p>
                                @if (prev_lang == "1")
                                {<strong>মাধ্যম:</strong>}
                                else
                                {<strong>Through:</strong>}

                                @if (prev_lang == "1")
                                {<span>মহাপরিচালক, জল সম্পদ পরিকল্পনা সংস্থা।</span>}
                                else
                                {<span>Director General, Water Resources Planning Organization.</span>}
                            </p>
                        </div>
                    </div>
                    <div class="row lineGap">
                        <div class="col">
                            <p>
                                @if (prev_lang == "1")
                                {<span>আমি, নীচে স্বাক্ষরিত, বন্যা নিয়ন্ত্রণ/ পরিচালনা সম্পর্কিত একটি প্রকল্প শুরু এবং বাস্তবায়নে ইচ্ছুক এবং আমি এখানে প্রাসঙ্গিক সংযুক্তিগুলির সাথে ছাড়পত্রের জন্য অনুরোধ করছি।</span>}
                                else
                                {<span>I, the undersigned, willing to initiate and implement a project on Flood Control/Management, And I am hereby requesting for clearance certificate with relevant attachments.</span>}
                            </p>
                        </div>
                    </div>
                    <div class="row lineGap">
                        <div class="col-md-12">
                            <p>
                                <strong>
                                    @if (prev_lang == "1")
                                    {<span>আবেদনকারীর পুরো নাম এবং ঠিকানা</span>}
                                    else
                                    {<span>Full Name and Address of Applicant</span>}
                                </strong>
                            </p>
                        </div>
                        <div class="col-md-12">
                            @if (prev_lang == "1")
                            {
                                <div>@ViewBag.ApplicantNameBn,</div>
                                <div>@ViewBag.ApplicantAddressBn</div>
                                <div>যোগাযোগ: @ViewBag.ApplicantMobileBn</div>
                                <div>ইমেইল: @ViewBag.ApplicantEmail</div>
                            }
                            else
                            {
                                <div>@ViewBag.ApplicantName,</div>
                                <div>@ViewBag.ApplicantAddress</div>
                                <div>Mobile: @ViewBag.ApplicantMobile</div>
                                <div>Email: @ViewBag.ApplicantEmail</div>
                            }
                        </div>
                    </div>

                    <h3>
                        @if (prev_lang == "1")
                        {<span>১. সাধারণ তথ্য</span>}
                        else
                        {<span>A. General Information</span>}
                    </h3>
                    <div class="row">
                        <div class="col">
                            <label for="pname">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>i. প্রকল্পের নাম</span>}
                                    else
                                    {<span>i. Project Name:</span>}
                                </b>
                            </label>
                        </div>
                        <div class="col">
                            <div title="Project Name">
                                <span id="pProjectName">xxxxxxxxxxxxxxxxxx</span> &nbsp;
                                <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Project Name', 'ProjectName');"
                                   class="btn btn-sm btn-info waves-effect comment" data-close="false"
                                   data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                                   style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                                    <i class="mdi mdi-comment-text-multiple"></i>
                                </a>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col">
                            <label for="pbg">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>ii. প্রকল্পের পূর্ব ইতিহাস ও যৌক্তিকতা</span>}
                                    else
                                    {<span>ii. Project Background and Rationale</span>}
                                </b>
                            </label>
                        </div>
                        <div class="col">
                            <span id="pBackgroundAndRationale" title="Background and Rationale">xxxxxxxxxxxxxxxxxx</span> &nbsp;
                            <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Project Background and Rationale', 'BackgroundAndRationale');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                               data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                               style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                                <i class="mdi mdi-comment-text-multiple"></i>
                            </a>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col">
                            <label for="pbg">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>iii. প্রকল্পের লক্ষ্য</span>}
                                    else
                                    {<span>iii. Project Target</span>}
                                </b>
                            </label>
                        </div>
                        <div class="col">
                            <span id="pProjectTarget">xxxxxxxxxxxxxxxxxx</span> &nbsp;
                            <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Project Target', 'ProjectTarget');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                               data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                               style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                                <i class="mdi mdi-comment-text-multiple"></i>
                            </a>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col">
                            <label for="pbg">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>iv. প্রকল্পের উদ্দেশ্য</span>}
                                    else
                                    {<span>iv. Project Objective</span>}
                                </b>
                            </label>
                        </div>
                        <div class="col">
                            <span id="pProjectObjective">xxxxxxxxxxxxxxxxxx</span> &nbsp;
                            <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Project Objective', 'ProjectObjective');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                               data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                               style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                                <i class="mdi mdi-comment-text-multiple"></i>
                            </a>
                        </div>
                    </div>

                    <div class="row">
                        <label for="pbg">
                            <b>
                                @if (prev_lang == "1")
                                {<span>iv. প্রকল্পের অবস্থান</span>}
                                else
                                {<span>iv. Project Location</span>}
                            </b>
                        </label>
                        &nbsp;
                        <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Project Location', 'ProjectLocation');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                           data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                           style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                            <i class="mdi mdi-comment-text-multiple"></i>
                        </a> &nbsp;
                        <a id="btnMapView" href="javascript:void(0);" target="_blank" class="btn btn-sm btn-info waves-effect comment"
                           data-toggle="tooltip" data-placement="top" title="" data-original-title="View Map"
                           style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                            <i class="mdi mdi-map"></i>
                        </a>
                    </div>

                    <div class="row">
                        <table style="width:100%">
                            <thead>
                                <tr>
                                    <th class="text-center">#</th>
                                    <th class="text-center">
                                        @if (prev_lang == "1")
                                        {<span>ইউনিয়ন</span>}
                                        else
                                        {<span>Union</span>}
                                    </th>
                                    <th class="text-center">
                                        @if (prev_lang == "1")
                                        {<span>উপজেলা</span>}
                                        else
                                        {<span>Upazila</span>}
                                    </th>
                                    <th class="text-center">
                                        @if (prev_lang == "1")
                                        {<span>জেলা</span>}
                                        else
                                        {<span>District</span>}
                                    </th>
                                    <th class="text-center">
                                        @if (prev_lang == "1")
                                        {<span>অক্ষাংশ</span>}
                                        else
                                        {<span>Latitude</span>}
                                    </th>
                                    <th class="text-center">
                                        @if (prev_lang == "1")
                                        {<span>দ্রাঘিমাংশ</span>}
                                        else
                                        {<span>Longitude</span>}
                                    </th>
                                    <th class="text-center">
                                        @if (prev_lang == "1")
                                        {<span>সংযুক্তি</span>}
                                        else
                                        {<span>Attchments</span>}
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="tbodyProjectLocationPreview"></tbody>
                        </table>
                    </div>
                    <br />

                    <div class="row">
                        <div class="col">
                            <label for="pbg">
                                <b>
                                    @if (prev_lang == "1")
                                    {<span>viii. প্রকল্প ব্যয়</span>}
                                    else
                                    {<span>viii. Estimated Project Cost</span>}
                                </b>
                            </label>
                        </div>
                        <div class="col">
                            <span id="pProjectEstimatedCost">xxxxxxxxxxxxxxxxxx</span> &nbsp;
                            <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Project Estimated Cost', 'ProjectEstimatedCost');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                               data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                               style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                                <i class="mdi mdi-comment-text-multiple"></i>
                            </a>
                        </div>
                    </div>

                    <label for="pbg">
                        <b>
                            @if (prev_lang == "1")
                            {<span>vii. প্রকল্প সময়কাল</span>}
                            else
                            {<span>vii. Project Period</span>}
                        </b>
                    </label> &nbsp;
                    <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Project Period', 'ProjectPeriod');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                       data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                       style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                        <i class="mdi mdi-comment-text-multiple"></i>
                    </a>
                    <table>
                        <tr>
                            <th>
                                @if (prev_lang == "1")
                                {<span>প্রাম্ভিক কাল</span>}
                                else
                                {<span>Starting time</span>}
                            </th>
                            <th>
                                @if (prev_lang == "1")
                                {<span>সমাপ্তি কাল</span>}
                                else
                                {<span>Completion time</span>}
                            </th>
                        </tr>
                        <tr>
                            <td id="pProjectStartDate">XX/XX/XXXX</td>
                            <td id="pProjectCompletionDate">XX/XX/XXXX</td>
                        </tr>
                    </table>
                    <br />

                    <h3>
                        @if (prev_lang == "1")
                        {<span>২. কারিগরি তথ্য</span>}
                        else
                        {<span>B. Technical Information</span>}
                    </h3>
                    <h4>
                        @if (prev_lang == "1")
                        {<span>১. বেজলাইন সম্পর্কিত তথ্য বা অবস্থা পর্যালোচনা</span>}
                        else
                        {<span>1. Discussion about baseline related information or situation</span>}
                    </h4>
                    <label for="pbg">
                        <b>
                            @if (prev_lang == "1")
                            {<span>হাইড্রোলজিক্যাল অঞ্চল</span>}
                            else
                            {<span>Hydrological Region</span>}
                        </b>
                    </label> &nbsp;
                    <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Hydrological Region', 'HydrologicalRegion');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                       data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                       style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                        <i class="mdi mdi-comment-text-multiple"></i>
                    </a>

                    <table>
                        <tr>
                            <td id="tdHydrologicalRegion-1" title="1" class="">
                                @if (prev_lang == "1")
                                {<span>উত্তর কেন্দ্রীয়</span>}
                                else
                                {<span>North Central</span>}
                            </td>
                            <td id="tdHydrologicalRegion-2" title="2" class="">
                                @if (prev_lang == "1")
                                {<span>উত্তরপূর্বকোণ</span>}
                                else
                                {<span>Northeast</span>}
                            </td>
                            <td id="tdHydrologicalRegion-3" title="3" class="">
                                @if (prev_lang == "1")
                                {<span>বায়ুকোণ</span>}
                                else
                                {<span>Northwest</span>}
                            </td>
                            <td id="tdHydrologicalRegion-4" title="4" class="">
                                @if (prev_lang == "1")
                                {<span>কেন্দ্রীয় দক্ষিণ</span>}
                                else
                                {<span>South Central </span>}
                            </td>
                            <td id="tdHydrologicalRegion-5" title="5" class="">
                                @if (prev_lang == "1")
                                {<span>দক্ষিণ-পূর্ব</span>}
                                else
                                {<span>Southeast</span>}
                            </td>
                            <td id="tdHydrologicalRegion-6" title="6" class="">
                                @if (prev_lang == "1")
                                {<span>নৈর্ঋত</span>}
                                else
                                {<span>Southwest</span>}
                            </td>
                            <td id="tdHydrologicalRegion-7" title="7" class="">
                                @if (prev_lang == "1")
                                {<span>নদী এবং মোহনা</span>}
                                else
                                {<span>River and Estuary </span>}
                            </td>
                            <td id="tdHydrologicalRegion-8" title="8" class="">
                                @if (prev_lang == "1")
                                {<span>পূর্ব পাহাড়</span>}
                                else
                                {<span>Eastern Hills </span>}
                            </td>
                        </tr>
                    </table>

                    <br />

                    <label for="pbg">
                        <b>
                            @if (prev_lang == "1")
                            {<span>vii. বাংলাদেশ ব-দ্বীপ পরিকল্পনা-২১০০ হট স্পট</span>}
                            else
                            {<span>vii. Bangladesh Delta Plan 2100 Hot Spot</span>}
                        </b>
                    </label> &nbsp;
                    <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Bangladesh Delta Plan 2100 Hot Spot', 'BDP2100HotSpot');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                       data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                       style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                        <i class="mdi mdi-comment-text-multiple"></i>
                    </a>
                    <table>
                        <tr>
                            <td id="tdBDP2100HotSpot-1" title="1" class="">
                                @if (prev_lang == "1")
                                {<span>উপকূলবর্তী এলাকা</span>}
                                else
                                {<span>Coastal Areas</span>}
                            </td>
                            <td id="tdBDP2100HotSpot-2" title="2" class="">
                                @if (prev_lang == "1")
                                {<span>বরেন্দ্র অঞ্চল ও খরা-প্রবণ অঞ্চল</span>}
                                else
                                {<span>Barind Tract and Drought-Prone Region </span>}
                            </td>
                            <td id="tdBDP2100HotSpot-3" title="3" class="">
                                @if (prev_lang == "1")
                                {<span>হাওর (backswamp) এবং ফ্ল্যাশ বন্যা-প্রবণ অঞ্চল</span>}
                                else
                                {<span>Haor (backswamp) and Flash Flood-Prone Areas </span>}
                            </td>
                            <td id="tdBDP2100HotSpot-4" title="4" class="">
                                @if (prev_lang == "1")
                                {<span>পার্বত্য চট্টগ্রাম অঞ্চল</span>}
                                else
                                {<span>Chittagong Hill Tract region </span>}
                            </td>
                            <td id="tdBDP2100HotSpot-5" title="5" class="">
                                @if (prev_lang == "1")
                                {<span>নদী অঞ্চল এবং মোহনা</span>}
                                else
                                {<span>River Region and Estuaries </span>}
                            </td>
                            <td id="tdBDP2100HotSpot-6" title="6" class="">
                                @if (prev_lang == "1")
                                {<span>নগর অঞ্চল</span>}
                                else
                                {<span>Urban Region</span>}
                            </td>
                        </tr>
                    </table><br />

                    <label for="pbg">
                        <b>
                            @if (prev_lang == "1")
                            {<span>iii. হাইড্রোলজিক্যাল সিস্টেম</span>}
                            else
                            {<span>iii. Hydrological System </span>}
                        </b>
                    </label> &nbsp;
                    <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Hydrological System', 'HydrologicalSystem');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                       data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                       style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                        <i class="mdi mdi-comment-text-multiple"></i>
                    </a>
                    <table>
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>
                                    @if (prev_lang == "1")
                                    {<span>হাইড্রোলজিক্যাল সিস্টেম</span>}
                                    else
                                    {<span>Hydrological System</span>}
                                </th>
                                <th>
                                    @if (prev_lang == "1")
                                    {<span>নাম</span>}
                                    else
                                    {<span>Name</span>}
                                </th>
                                <th>
                                    @if (prev_lang == "1")
                                    {<span>দৈর্ঘ্য বা অঞ্চল</span>}
                                    else
                                    {<span>Length or Area</span>}
                                </th>
                            </tr>
                        </thead>
                        <tbody id="tbodyHydrologicalSystemPreview"></tbody>
                    </table>
                    <br />

                    <label for="pbg">
                        <b>
                            @if (prev_lang == "1")
                            {<span>iv. খাল, নদী এবং জলাভূমির মধ্যে সংযোগ</span>}
                            else
                            {<span>iv. Connectivity amid khals, river and wetland</span>}
                        </b>
                    </label>
                    <span id="pConnectivityAmidWaterland">xxxxxxxxxxxxxxxxxx</span> &nbsp;
                    <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Connectivity Amid Khals, River and Wetland', 'ConnectivityAmidWaterland');" class="btn btn-sm btn-info waves-effect comment" data-close="false"
                       data-animation="fadein" data-plugin="custommodal" data-overlayColor="#38414a"
                       style="margin: 0px !important; padding: 05px !important; line-height: 1 !important;">
                        <i class="mdi mdi-comment-text-multiple"></i>
                    </a>
                    <br />

                    <label for="pbg">
                        <b>
                            @if (prev_lang == "1")
                            {<span>v. ক্যাচমেন্ট এলাকা (অববাহিকা) (হেক্টর)</span>}
                            else
                            {<span>v. Catchment Area (ha)</span>}
                        </b>
                    </label>
                    <span id="pCatchmentArea">xxxxxxxxxxxxxxxxxx</span> &nbsp;
                    <a href="#control-popup-comments" onclick="javascript:setInfoInPopup('Catchment Area (ha)', 'CatchmentArea');"
