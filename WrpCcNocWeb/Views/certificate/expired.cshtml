@{
    ViewData["Title"] = "Expired";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row justify-content-center">
    <div class="col-lg-6 col-xl-4 mb-4">
        <div class="error-text-box">
            <svg viewBox="0 0 750 200">
                <!-- Symbol-->
                <symbol id="s-text">
                    <text text-anchor="middle" x="50%" y="50%" dy=".35em">EXPIRED</text>
                </symbol>
                <!-- Duplicate symbols-->
                <use class="text" xlink:href="#s-text"></use>
                <use class="text" xlink:href="#s-text"></use>
                <use class="text" xlink:href="#s-text"></use>
                <use class="text" xlink:href="#s-text"></use>
                <use class="text" xlink:href="#s-text"></use>
            </svg>
        </div>
        <div class="text-center">
            <h3 class="mt-0 mb-2">Certificate Expired</h3>
            <p class="text-muted mb-3">
                Sorry, your Online Water Resources Project Clearance of Bangladesh certificate has been expired!
            </p>

            <a href="@Url.Action("list", "form")" class="btn btn-success waves-effect waves-light">Back to List</a>
        </div>
        <!-- end row -->

    </div> <!-- end col -->
</div>