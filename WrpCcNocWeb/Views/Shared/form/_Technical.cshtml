@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    var language = HttpContextAccessor.HttpContext.Request.Cookies["FormLanguage"].ToString();
}

<div class="card-box">
    <div id="tech-info-form">
        <div class="card">
            <div class="card-header bg-blue py-3 text-white cursor" data-toggle="collapse" href="#cardCollpase31">
                <div class="card-widgets">
                    <a data-toggle="collapse" href="#cardCollpase31" role="button" aria-expanded="false" aria-controls="cardCollpase2"><i class="mdi mdi-minus"></i></a>
                </div>
                <h5 class="card-title mb-0 text-white">
                    @if (language == "1")
                    {
                        <span>১. বেজলাইন সম্পর্কিত তথ্য বা অবস্থা পর্যালোচনা</span>}
                    else
                    {
                        <span>1. Discussion about baseline related information or situation</span>}
                </h5>
            </div>

            <div id="cardCollpase31" class="collapse show">
                <div class="card-body">
                    <div class="form-group">
                        <label for="HydrologicalRegion">
                            @if (language == "1")
                            {<span>i. হাইড্রোলজিক্যাল অঞ্চল</span>}
                            else
                            {<span>i. Hydrological Region</span>}
                        </label>
                        <div id="chkHydrologicalRegions" style="margin-left: 5px;">
                            @{
                                List<LookUpCcModHydroRegion> _hydro_regions = ViewBag.LookUpCcModHydroRegion;

                                if (_hydro_regions.Count > 0)
                                {
                                    foreach (LookUpCcModHydroRegion hr in _hydro_regions)
                                    {
                                        var id = "chkHydroRegion" + hr.HydroRegionId;

                                        if (language == "1")
                                        {
                                            <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullNameBn">
                                                <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                <label for="@id"> @hr.HydroRegionFullNameBn </label>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hr.HydroRegionFullName">
                                                <input type="checkbox" name="hydrological_region" id="@id" value="@hr.HydroRegionId" data-parsley-multiple="hydrological_region">
                                                <label for="@id"> @hr.HydroRegionFullName </label>
                                            </div>
                                        }
                                    }
                                }
                            }
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="BDP2100HotSpot">
                            @{if (language == "1")
                                { <span>ii. বাংলাদেশ ব-দ্বীপ পরিকল্পনা-২১০০  হট স্পট</span>}
                                else
                                {<span>ii. Bangladesh Delta Plan 2100 Hot Spot </span>}
                            }
                        </label>
                        <div id="chkBDP2100HotSpot" class="row" style="margin-left: 5px;">
                            @{
                                List<LookUpCcModDeltPlan2100HotSpot> _hot_spots = ViewBag.LookUpCcModDeltPlan2100HotSpot;

                                if (_hot_spots.Count > 0)
                                {
                                    foreach (LookUpCcModDeltPlan2100HotSpot hs in _hot_spots)
                                    {
                                        var id = "chkBDP2100HotSpot" + hs.DeltaPlanHotSpotId;

                                        if (language == "1")
                                        {
                                            <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanNameBn">
                                                <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                <label for="@id"> @hs.PlanNameBn </label>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="checkbox checkbox-pink mb-1 form-check-inline col-md-5" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@hs.PlanName">
                                                <input type="checkbox" name="bdp2100_hot_spot" id="@id" value="@hs.DeltaPlanHotSpotId" data-parsley-multiple="bdp2100_hot_spot">
                                                <label for="@id"> @hs.PlanName </label>
                                            </div>
                                        }
                                    }
                                }
                            }
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="NameOfRiverWaterCourse">
                            @{if (language == "1")
                                { <span>iii. নদীর/প্রবাহিত পানির নাম</span>}
                                else
                                {<span>iii. Name of River/ Water Course</span>}
                            }
                        </label>
                        <input type="text" id="NameOfRiverWaterCourse" name="NameOfRiverWaterCourse" class="form-control" />
                    </div>

                    <div class="form-group">
                        <label for="RiverNature">
                            @{if (language == "1")
                                { <span>iv. নদীর ধরণ</span> }
                                else
                                { <span>iv. River Nature</span> }
                            }
                        </label>
                        <div id="RiverNature" class="row" style="margin-left: 5px;">
                            @{
                                List<LookUpCcModRiverNature> _rvnt = ViewBag.LookUpCcModRiverNature;

                                if (_rvnt.Count > 0)
                                {
                                    foreach (LookUpCcModRiverNature rn in _rvnt)
                                    {
                                        var id = "radRiverNature" + rn.RiverNatureId;

                                        if (language == "1")
                                        {
                                            <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                <input type="radio" name="RiverNatureId" id="@id" value="@rn.RiverNatureId" data-parsley-multiple="RiverNatureId">
                                                <label for="@id"> @rn.RiverNatureTitleBn </label>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                <input type="radio" name="RiverNatureId" id="@id" value="@rn.RiverNatureId" data-parsley-multiple="RiverNatureId">
                                                <label for="@id"> @rn.RiverNatureTitle </label>
                                            </div>
                                        }
                                    }
                                }
                            }
                        </div>
                    </div>

                    <div id="divBankErosion">
                        <label for="BankErosion" class="bold">
                            @{if (language == "1")
                                { <span>v. নদী ভাঙ্গন</span>}
                                else
                                {<span>v. Bank Erosion</span>}
                            }
                        </label>

                        <hr style="margin-top: 0 !important;" />

                        <div class="row">
                            <div class="form-group col-xl-4 text-center">
                                <label for="BankErosionLength">
                                    @if (language == "1")
                                    {<span>দৈঘ্য (মিটার)</span> }
                                    else
                                    {<span>Length (m)</span>}
                                </label>
                                <input type="number" min="0" id="BankErosionLength" name="BankErosionLength" class="form-control" />
                            </div>

                            <div class="form-group col-xl-4 text-center">
                                <label for="BankErosionArea">
                                    @if (language == "1")
                                    {<span>এরিয়া (বঃ মিঃ)	</span> }
                                    else
                                    { <span>Area (ha)</span>}
                                </label>
                                <input type="number" min="0" id="BankErosionArea" name="BankErosionArea" class="form-control" />
                            </div>

                            <div class="form-group col-xl-4 text-center">
                                <label for="BankErosionLocation">
                                    @if (language == "1")
                                    {<span>স্থান</span> }
                                    else
                                    { <span>Location</span>}
                                </label>
                                <input type="text" id="BankErosionLocation" name="BankErosionLocation" class="form-control" />
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="BankErosionRate">
                            @{if (language == "1")
                                { <span>vi. ভাঙ্গনের হার (মি./বছর)</span>}
                                else
                                {<span>vi. Erosion rate (m/year)</span>}
                            }
                        </label>
                        <input type="number" min="0" id="BankErosionRate" name="BankErosionRate" class="form-control" />
                    </div>

                    <div class="form-group">
                        <label for="SetBackDistanceImpStruct">
                            @{if (language == "1")
                                { <span>vii. গুরুত্বপূর্ণ অবকাঠামো / রাস্তা / ভবন হতে নদীর তীরের দূরত্ব</span>}
                                else
                                {<span>vii. Set back distance from important structure/road/building/Local habitation</span>}
                            }
                        </label>
                        <input type="text" id="SetBackDistanceImpStruct" name="SetBackDistanceImpStruct" class="form-control" />
                    </div>

                    <div class="form-group">
                        <label for="SoilTypeRiverBank">
                            @{if (language == "1")
                                { <span>viii. নদীর তীরের মাটির প্রকৃতি</span>}
                                else
                                {<span>viii. Soil type of river bank</span>}
                            }
                        </label>
                        <input type="text" id="SoilTypeRiverBank" name="SoilTypeRiverBank" class="form-control" />
                    </div>

                    <div class="form-group">
                        <label for="SedimentationRiverKhal">
                            @{if (language == "1")
                                {<span>ix. নদীাতে পলি জমার (সেডিমেন্টেশন) পরিমাণ</span>}
                                else
                                {<span>ix. Sedimentation of Rivers</span>}
                            }
                        </label>
                        <div id="SedimentationRiverKhal" class="row" style="margin-left: 5px;">
                            @{
                                List<LookUpCcModSediOfRiverOrKhal> _sork = ViewBag.LookUpCcModSediOfRiverOrKhal;

                                if (_sork.Count > 0)
                                {
                                    foreach (LookUpCcModSediOfRiverOrKhal rk in _sork)
                                    {
                                        var id = "radSediOfRiverOrKhal" + rk.SedimentationId;

                                        if (language == "1")
                                        {
                                            <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                <input type="radio" name="SedimentationId" id="@id" value="@rk.SedimentationId" data-parsley-multiple="SedimentationId">
                                                <label for="@id"> @rk.SedimentationNameBn </label>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="radio radio-info form-check-inline" style="margin-right: 25px;">
                                                <input type="radio" name="SedimentationId" id="@id" value="@rk.SedimentationId" data-parsley-multiple="SedimentationId">
                                                <label for="@id"> @rk.SedimentationName </label>
                                            </div>
                                        }
                                    }
                                }
                            }
                        </div>
                    </div>

                    <div id="divDevelopmentOfChars">
                        <label for="DevelopmentOfChars" class="bold">
                            @{if (language == "1")
                                { <span>x. চর সমূহের বিকাশতা</span>}
                                else
                                {<span>x. Development of Chars</span>}
                            }
                        </label>

                        <hr style="margin-top: 0 !important;" />

                        <div class="row">
                            <div class="form-group col-xl-6 text-center">
                                <label for="CharArea">
                                    @if (language == "1")
                                    {<span>এরিয়া (হেক্টর)</span> }
                                    else
                                    {<span>Area (ha)</span>}
                                </label>
                                <input type="number" min="0" id="CharArea" name="CharArea" class="form-control" />
                            </div>

                            <div class="form-group col-xl-6 text-center">
                                <label for="CharLocation">
                                    @if (language == "1")
                                    {<span>অবস্থান</span> }
                                    else
                                    { <span>Location</span>}
                                </label>
                                <input type="text" id="CharLocation" name="CharLocation" class="form-control" />
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="IrrigatedCropArea">
                            @{if (language == "1")
                                { <span>xi. খাদ্যশস্য চাষাবাদকৃত এলাকা (হেক্টর)</span>}
                                else
                                {<span>xi. Irrigated Crop Area (ha)</span>}
                            }
                        </label>
                        <input type="number" min="0" id="IrrigatedCropArea" name="IrrigatedCropArea" class="form-control" />
                    </div>

                    <div class="form-group">
                        <label for="CropProduction">
                            @{if (language == "1")
                                { <span>xii. ফসল/ খাদ্যশস্য উৎপাদনের পরিমাণ (টন)</span>}
                                else
                                {<span>xii. Crop Production (ton)</span>}
                            }
                        </label>
                        <input type="number" min="0" id="CropProduction" name="CropProduction" class="form-control" />
                    </div>

                    <div class="form-group">
                        <label for="FloraAndFauna">
                            @{if (language == "1")
                                { <span>xiii. প্রকল্প এলাকায় বিদ্যমান গাছপালা ও পশুপাখি</span>}
                                else
                                {<span>xiii.  Flora and Fauna</span>}
                            }
                        </label>
                        <input type="text" id="FloraAndFauna" name="FloraAndFauna" class="form-control" />
                    </div>

                    <div id="divSocioEconomicCondition">
                        <label for="SocioEconomicCondition" class="bold">
                            @{if (language == "1")
                                { <span>xiv. আর্থ সামাজিক অবস্থা</span>}
                                else
                                {<span>xiv. Socio Economic Condition</span>}
                            }
                        </label>

                        <hr style="margin-top: 0 !important;" />

                        <div class="row">
                            <div class="form-group col-xl-4 text-center">
                                <label for="SecLandLessPeople">
                                    @if (language == "1")
                                    {<span>ভূমি হীন জনগোষ্ঠী (শতকরা)</span> }
                                    else
                                    { <span>% of Land Less People</span>}
                                </label>
                                <input type="number" min="0" id="SecLandLessPeople" name="SecLandLessPeople" class="form-control" />
                            </div>

                            <div class="form-group col-xl-4 text-center">
                                <label for="SecSmallFarmer">
                                    @if (language == "1")
                                    {<span>ছোট / প্রান্তিক কৃষক (শতকরা)</span> }
                                    else
                                    { <span>% of Small Farmer</span>}
                                </label>
                                <input type="number" min="0" id="SecSmallFarmer" name="SecSmallFarmer" class="form-control" />
                            </div>

                            <div class="form-group col-xl-4 text-center">
                                <label for="SecAvgMonthlyIncome">
                                    @if (language == "1")
                                    {<span>মাসিক গড় আয়</span> }
                                    else
                                    { <span>Average Monthly Income</span>}
                                </label>
                                <input type="text" id="SecAvgMonthlyIncome" name="SecAvgMonthlyIncome" class="form-control" />
                            </div>
                        </div>
                    </div>
                </div>

                <div class="card">
                    <div class="card-header bg-warning py-3 text-white cursor" data-toggle="collapse" href="#cardCollpase382">
                        <div class="card-widgets">
                            <a data-toggle="collapse" href="#cardCollpase382" role="button" aria-expanded="false" aria-controls="cardCollpase382"><i class="mdi mdi-minus"></i></a>
                        </div>
                        <h5 class="card-title mb-0 text-white">
                            @if (language == "1")
                            {
                                <span>২. রিভার মর্ফোলজি ও রিভার হাইড্রোলজি অবস্থা</span>}
                            else
                            {
                                <span>2. Status of River Morphology and River Hydrology</span>}
                        </h5>
                    </div>
                    <div id="cardCollpase382" class="collapse show">
                        <div class="card-body">
                            <div class="form-group">
                                <label for="RiverNature">
                                    @{if (language == "1")
                                        {<span>i. নদীর ধরণ</span>}
                                        else
                                        {<span>i. River Type</span>}
                                    }
                                </label>
                                <div id="RiverNature" class="row" style="margin-left: 5px;">
                                    @{
                                        List<LookUpCcModRiverType> _rvin = ViewBag.LookUpCcModRiverType;

                                        if (_rvin.Count > 0)
                                        {
                                            foreach (LookUpCcModRiverType rn in _rvin)
                                            {
                                                var id = "chkRiverType" + rn.RiverTypeId;

                                                if (language == "1")
                                                {
                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@rn.RiverTypeNameBn">
                                                        <input type="checkbox" name="RiverTypeId" id="@id" value="@rn.RiverTypeId" data-parsley-multiple="RiverTypeId">
                                                        <label for="@id"> @rn.RiverTypeNameBn </label>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="checkbox checkbox-pink mb-1 form-check-inline" style="margin-right: 25px;" data-toggle="tooltip" data-placement="top" title="" data-original-title="@rn.RiverTypeName">
                                                        <input type="checkbox" name="RiverTypeId" id="@id" value="@rn.RiverTypeId" data-parsley-multiple="RiverTypeId">
                                                        <label for="@id"> @rn.RiverTypeName </label>
                                                    </div>
                                                }
                                            }
                                        }
                                    }
                                </div>
                            </div>

                            <div id="divSocioEconomicCondition">
                                <label for="SocioEconomicCondition" class="bold">
                                    @{if (language == "1")
                                        { <span>ii. নদীর দৈর্ঘ্য এবং প্রস্থ (মি.)</span>}
                                        else
                                        {<span>ii. River Depth and Width (m)</span>}
                                    }
                                </label>

                                <hr style="margin-top: 0 !important;" />

                                <div class="row">
                                    <div class="form-group col-xl-4 text-center">
                                        <label for="RiverDepth">
                                            @if (language == "1")
                                            {<span>দৈর্ঘ্য</span> }
                                            else
                                            { <span>Depth</span>}
                                        </label>
                                        <input type="number" min="0" id="RiverDepth" name="RiverDepth" class="form-control" />
                                    </div>

                                    <div class="form-group col-xl-4 text-center">
                                        <label for="RiverWidth">
                                            @if (language == "1")
                                            {<span>প্রস্থ</span> }
                                            else
                                            { <span>Width</span>}
                                        </label>
                                        <input type="number" min="0" id="RiverWidth" name="RiverWidth" class="form-control" />
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="SedimentationRate">
                                    @{if (language == "1")
                                        { <span>iii. নদী/ খালে পলি প্রবাহের হার (সেডিমেন্টেশন রেট); (সে.মি./ বছর) </span>}
                                        else
                                        {<span>iii. Sedimentation Rate of River/ Khal (cm/ year)</span>}
                                    }
                                </label>
                                <input type="number" min="0" id="SedimentationRate" name="SedimentationRate" class="form-control" />
                            </div>

                            <div class="form-group">
                                <label for="WaterLevelIntakePoint">
                                    @{if (language == "1")
                                        { <span>iv. পানির সমতল (ওয়াটার লেভেল)</span>}
                                        else
                                        {<span>iv. Water Level (m PWD)</span>}
                                    }
                                </label>

                                <div id="WaterLevelIntakePoint" class="row" style="margin-left: -7px;">
                                    <div class="table-responsive">
                                        <table class="table mb-0">
                                            <thead>
                                                <tr>
                                                    <th class="text-center" colspan="2">Dry Season</th>
                                                    <th class="text-center" colspan="2">Wet Season</th>
                                                </tr>
                                                <tr>
                                                    <th class="text-center">Maximum</th>
                                                    <th class="text-center">Minimum</th>
                                                    <th class="text-center">Maximum</th>
                                                    <th class="text-center">Minimum</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td class="text-center">
                                                        <input type="number" id="WaterLevelDryMax" name="WaterLevelDryMax" class="form-control" />
                                                    </td>
                                                    <td class="text-center">
                                                        <input type="number" id="WaterLevelDryMin" name="WaterLevelDryMin" class="form-control" />
                                                    </td>
                                                    <td class="text-center">
                                                        <input type="number" id="WaterLevelWetMax" name="WaterLevelWetMax" class="form-control" />
                                                    </td>
                                                    <td class="text-center">
                                                        <input type="number" id="WaterLevelWetMin" name="WaterLevelWetMin" class="form-control" />
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="DischargeIntakePoint">
                                    @{if (language == "1")
                                        { <span>v. পানির প্রবাহ (ডিসচার্জ); (কিউমেক)</span>}
                                        else
                                        {<span>v. Discharge (m<sup>3</sup>/s)</span>}
                                    }
                                </label>

                                <div id="DischargeIntakePoint" class="row" style="margin-left: -7px;">
                                    <div class="table-responsive">
                                        <table class="table mb-0">
                                            <thead>
                                                <tr>
                                                    <th class="text-center" colspan="2">Dry Season</th>
                                                    <th class="text-center" colspan="2">Wet Season</th>
                                                </tr>
                                                <tr>
                                                    <th class="text-center">Maximum</th>
                                                    <th class="text-center">Minimum</th>
                                                    <th class="text-center">Maximum</th>
                                                    <th class="text-center">Minimum</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td class="text-center">
                                                        <input type="number" id="DischargeDryMax" name="DischargeDryMax" class="form-control" />
                                                    </td>
                                                    <td class="text-center">
                                                        <input type="number" id="DischargeDryMin" name="DischargeDryMin" class="form-control" />
                                                    </td>
                                                    <td class="text-center">
                                                        <input type="number" id="DischargeWetMax" name="DischargeWetMax" class="form-control" />
                                                    </td>
                                                    <td class="text-center">
                                                        <input type="number" id="DischargeWetMin" name="DischargeWetMin" class="form-control" />
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

