@using WrpCcNocWeb.Models.UserManagement

@{
    ViewData["Title"] = "Home page";
}

@{
    UserLevelInfo uli = ViewBag.UserLevelInfo;

    if (uli.UserGroupId.ToString() == "1000000001" && uli.AuthorityLevelId == 0)
    {
        @Html.Partial("dashboards/_Requester")
    }
    else if (uli.UserGroupId.ToString() != "1000000001" && uli.AuthorityLevelId == 0)
    {
        @Html.Partial("dashboards/_Warpo")
    }
    else
    {
        @Html.Partial("dashboards/_Authority")
    }
}

@section PageScripts {
    <script src="~/js/pages/dashboard-1.init.js" type="text/javascript"></script>

    <script type="text/javascript">
        jQuery(document).ready(function () {

        });
    </script>
}